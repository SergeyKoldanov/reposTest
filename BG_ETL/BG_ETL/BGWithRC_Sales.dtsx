                <property
                  dataType="System.String"
                  description="Команда SQL для выполнения."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">declare @date1 date, @date2 date;
set @date1 = DATEADD(month, DATEDIFF(month, 0, dateadd(m, -3, getdate())), 0)
set @date2 = cast(getdate() as date);

WITH Kontr AS 
			 (
			  SELECT [id], [Фирма], [Фирма] AS [Сеть]
			  FROM firms witn (nolock)
			  WHERE id in (130, 118905, 118906, 118908, 118910, 119423, 121485, 55747, 61457)

				UNION ALL
	  
			  SELECT C.[id], C.[Фирма], Kontr.[Сеть]
			  FROM Kontr INNER JOIN firms C (nolock)
				ON Kontr.id = C.Parentid
			 )

, Clients AS (
select
	  id, Фирма, Сеть as Направление
from kontr f left join bgclients b
on f.id = b.rcid)

select sum(kol) as kol
, sum(oborot) as oborot
, DATEADD(month, DATEDIFF(month, 0, data), 0) as Period
, k.Description		as cat
, kk.Description	as class
, p2.Description	as [level 02]
, p.Description		as [level 03]
, cast(case 
  when g.rcid is null and b.Производитель != 'БиДжи' then '_BG'
  when b.Производитель = 'БиДжи' then 'BG'
  when  g.rcid is not null and b.Производитель != 'БиДжи' then '_BG'
  end as nvarchar) as TM
, case
  when g.rcid is null then 'ОстальныеКлиентыБиДжи '
  else g.headclient end as client
, c.Направление as direction
, case
  when g.rcid is null then 'РЦ'
  else g.организация end as company
from [vw_ProdazhiBezProdFil_Prodazhi] a (nolock)
join Clients c (nolock)
	on c.id = a.KontragentID
left join goods b (nolock)
	on a.NomenklaturaID = b.ID
left join vwKlassifKategoriya k (nolock)
	on b.KlassifKategoriyaID = k.Id
left join vwKlassifKlass kk (nolock)
	on b.KlassifKlassID = kk.Id
left join vwKlassifProduktTovar p (nolock)
	on b.KlassifProduktTovarID = p.Id
left join vwKlassifProduktTovar p2 (nolock)
	on p2.Id = p.ParentID
left join bgclients g (nolock)
	on g.rcid = a.GolovnojKontragID
where a.data &gt;= @date1
group by DATEADD(month, DATEDIFF(month, 0, data), 0)
, k.Description
, kk.Description
, p2.Description
, p.Description
, c.Направление
, case
  when g.rcid is null then 'ОстальныеКлиентыБиДжи '
  else g.headclient end
, case
  when g.rcid is null then 'РЦ'
  else g.организация end
, case 
  when g.rcid is null and b.Производитель != 'БиДжи' then '_BG'
  when b.Производитель = 'БиДжи' then 'BG'
  when  g.rcid is not null and b.Производитель != 'БиДжи' then '_BG'
  end</property>
