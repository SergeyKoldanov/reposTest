		AND Period &gt;= DATEADD(DAY,1,EOMONTH(getdate(),-2)) and Period &lt;= DATEADD(DAY,DAY(EOMONTH(GETDATE(), -1)),EOMONTH(getdate(),-2))
     ) pred
