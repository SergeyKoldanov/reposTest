SELECT cast('ФактПредыдущийГод' as nvarchar) as Тип, O.Код1с, O.ГоловнойКлиент, O.МенеджерКлиента
   ,O.Подразделение
   ,O.Регион, O.tec, O.TradeDirection
FROM Obschet2020 O
WHERE tec &gt;= 10000 and (
	 mec12 = 0 
	 or (pred &lt; 10000 and pred &gt; 0 and pred12 = 0) 
	 or (predpred &lt; 10000 and predpred &gt; 0 and predpred12 = 0)
