                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH Month26 AS (
SELECT 
	   Code           AS [Код1с]
      ,[Description]  AS [ГоловнойКлиент]
      ,Summa        AS [Оборот]
	  ,P.Period
	  ,(
           SELECT pol.[Description]
           FROM   [RZN-OLAP].[dw].[dbo].hrSprPolzovateli pol(NOLOCK)
                  JOIN [RZN-OLAP].[dw].[dbo].hrSprKontragent g(NOLOCK)
                       ON  g.Menedzher = pol.IDRRef
                           AND g.IDRRef = p.IDRRef
       )              AS [МенеджерКлиента]
      ,(
           SELECT pod.[Description]
           FROM   [RZN-OLAP].[dw].[dbo].hrSprPodrazdeleniya pod(NOLOCK)
           WHERE  pod.id = PodrazdelenieID
       )              AS [Подразделение]
      ,(
           SELECT r.[Description]
           FROM   [RZN-OLAP].[dw].[dbo].hrSprRegiony r(NOLOCK)
                  JOIN [RZN-OLAP].[dw].[dbo].hrSprKontragent gk(NOLOCK)
                       ON  r.IDRRef = gk.Region
           WHERE  gk.IDRRef = p.IDRRef
       )              AS [Регион]
FROM   (
           SELECT k.Code
                 ,k.IDRRef
                 ,k.[Description]
                 ,p.PodrazdelenieID
                 ,isnull(SUM(p.Oborot),  0)  Summa
				 ,DATEADD(DAY,1,EOMONTH(p.Period, -1)) as Period
           FROM   [RZN-OLAP].[dw].[dbo].hrProdazhiTek p(NOLOCK)
                  JOIN [RZN-OLAP].[dw].[dbo].hrSprKontragent ko(NOLOCK)
                       ON  ko.id = p.KontragentID
                  JOIN [RZN-OLAP].[dw].[dbo].hrSprKontragent k(NOLOCK)
                       ON  k.IDRRef = ko.GolovnojKontrag
                  JOIN [RZN-OLAP].[WORK].[dbo].tbKlientVGruppe kvg(NOLOCK)
                       ON  kvg.KontragentID = k.ID
                  JOIN [RZN-OLAP].[WORK].[dbo].tbTovarVGruppe AS tvg(NOLOCK)
                       ON  tvg.NomenklaturaID = p.NomenklaturaID
                           AND tvg.GruppaID IN (127071 -- Канцелярские товары
                                               ,127072 -- Товары офисного назначения (ТОН)
                                                )
           WHERE  p.Period &gt;= DATEADD(DAY,1,EOMONTH(getdate(),-27)) AND p.Period &lt;= dateadd(MONTH, -12, cast(getdate() as date))
                  AND kvg.GruppaID IN ( 117267 -- B2B
									  ,118905 -- Дистрибьюция
									  ,118910 -- Сетевой отдел
									  ,118908 -- Художественная дистрибьюция
									  ,118906 -- Экспорт
                                       )
					
           GROUP BY
                  k.code
                 ,k.IDRRef
                 ,k.[Description]
                 ,p.PodrazdelenieID
				 ,DATEADD(DAY,1,EOMONTH(p.Period, -1))
	) P
),

Obschet2020 AS (
SELECT 
	Код1с
   ,ГоловнойКлиент
   ,МенеджерКлиента
   ,Подразделение
   ,Регион
   ,C.TradeDirection
   ,(
     SELECT isnull(SUM(T.Оборот), 0)
     FROM Month26 T
     WHERE T.Код1с = M.Код1с
		AND Period &gt;= DATEADD(DAY,1,EOMONTH(getdate(),-13)) and Period &lt;= dateadd(MONTH, -12, cast(getdate() as date))
     ) tec
   ,(
     SELECT isnull(SUM(H.Оборот), 0)
     FROM Month26 H
     WHERE H.Код1с = M.Код1с
		AND Period &gt;= DATEADD(DAY,1,EOMONTH(getdate(),-25)) and Period &lt;= DATEADD(DAY,DAY(EOMONTH(GETDATE(), -1)),EOMONTH(getdate(),-14))
     ) mec12
   ,(
     SELECT isnull(SUM(G.Оборот), 0)
     FROM Month26 G
     WHERE G.Код1с = M.Код1с
		AND Period &gt;= DATEADD(DAY,1,EOMONTH(getdate(),-14)) and Period &lt;= DATEADD(DAY,DAY(EOMONTH(GETDATE(), -1)),EOMONTH(getdate(),-14))
     ) pred
   ,(
     SELECT isnull(SUM(F.Оборот), 0)
     FROM Month26 F
     WHERE F.Код1с = M.Код1с
		AND Period &gt;= DATEADD(DAY,1,EOMONTH(getdate(),-26)) and Period &lt;= DATEADD(DAY,DAY(EOMONTH(GETDATE(), -2)),EOMONTH(getdate(),-15))
     ) pred12
   ,(
     SELECT isnull(SUM(C.Оборот), 0)
     FROM Month26 C
     WHERE C.Код1с = M.Код1с
		AND Period &gt;= DATEADD(DAY,1,EOMONTH(getdate(),-15)) and Period &lt;= DATEADD(DAY,DAY(EOMONTH(GETDATE(), -2)),EOMONTH(getdate(),-15))
     ) predpred
   ,(
     SELECT isnull(SUM(J.Оборот), 0)
     FROM Month26 J
     WHERE J.Код1с = M.Код1с
		AND Period &gt;= DATEADD(DAY,1,EOMONTH(getdate(),-27)) and Period &lt;= DATEADD(DAY,DAY(EOMONTH(GETDATE(), -3)),EOMONTH(getdate(),-16))
     ) predpred12
FROM Month26 M left join ClientManager C ON M.Код1с = C.HeadClientCode
group by 	Код1с
   ,ГоловнойКлиент
   ,МенеджерКлиента
   ,Подразделение
   ,Регион
   ,C.TradeDirection
)

