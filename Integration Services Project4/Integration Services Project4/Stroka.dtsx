                <property
                  dataType="System.String"
                  description="Команда SQL для выполнения."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH Stroki AS (
SELECT
  DATEADD(DAY,1,EOMONTH(h.Period,-1)) [Period]
 ,count(h.NomenklaturaID) as PosCount
 ,sum(CASE
    WHEN h.Kol &gt;= 0 THEN 1
    ELSE 0
  END) PosSaleCount
  ,n.Code AS ItemCode
 ,cast(sum(CASE
    WHEN h.Oborot &lt; 0 THEN 0
    ELSE h.Sebest
  END) as float) AS NetPrice
,cast(kl2.Code as nvarchar(10)) AS HeadClientCode 

,h.SkladID
FROM [RZN-OLAP].[DW].[dbo].hrProdazhiTek h (NOLOCK)
JOIN [RZN-OLAP].[DW].[dbo].hrSprNomenklatura n (NOLOCK)
  ON h.[NomenklaturaID] = n.id
LEFT JOIN [RZN-OLAP].[DW].[dbo].hrSprKontragent kl (NOLOCK)
  ON kl.id = h.KontragentID
LEFT JOIN [RZN-OLAP].[DW].[dbo].hrSprKontragent kl2 (NOLOCK)
  ON kl2.IDRRef = kl.GolovnojKontrag
LEFT JOIN [RZN-OLAP].[DW].[dbo].[hrSprSklady] s (NOLOCK) 
  ON h.SkladID = s.id

  WHERE h.Period &gt;= '20190101' AND s.Description not like '%Транзитный%'
  GROUP BY DATEADD(DAY,1,EOMONTH(h.Period,-1)), cast(kl2.Code as nvarchar(10)), n.Code, h.SkladID
)

select
t2.Department as Подразделение, t3.Direction as Товарная_группа, 
sum([PosSaleCount]) as Значение, cast('Количество позиций продаж' as nvarchar) as 'Показатель', Period
from Stroki t1
LEFT JOIN lg..[ClientManager] t2 on t2.HeadClientCode = t1.HeadClientCode
LEFT JOIN lg..[Items] t3 on t3.Code = t1.ItemCode
WHERE t3.Direction is not null
Group by t2.Department, t3.Direction, Period

Union all
select
t2.Department as Подразделение, t3.Direction as Товарная_группа, 
sum([NetPrice]) as Значение, cast('Себестоимость продаж' as nvarchar) as 'Показатель', Period
from Stroki t1
LEFT JOIN lg..[ClientManager] t2 on t2.HeadClientCode = t1.HeadClientCode
LEFT JOIN lg..[Items] t3 on t3.Code = t1.ItemCode
WHERE t3.Direction is not null
