                <property
                  dataType="System.String"
                  description="Команда SQL для выполнения."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH Stroki AS (
SELECT
		  DATEADD(DAY,1,EOMONTH(h.Period,-1)) [Period]
		 ,COUNT(h.NomenklaturaID) AS PosCount
		 ,SUM(CASE
				WHEN h.Kol &gt;= 0 
				THEN 1
				ELSE 0
			  END) PosSaleCount
		  ,n.Code AS ItemCode
		 ,CAST(SUM(CASE
					WHEN h.Oborot &lt; 0 
					THEN 0
					ELSE h.Sebest
				  END) AS float) AS NetPrice
		,CAST(kl2.Code AS nvarchar(10)) AS HeadClientCode 
		,h.SkladID
FROM [RZN-OLAP].[DW].[dbo].hrProdazhiTek h (NOLOCK)
		JOIN [RZN-OLAP].[DW].[dbo].hrSprNomenklatura n (NOLOCK)
		  ON h.[NomenklaturaID] = n.id
		LEFT JOIN [RZN-OLAP].[DW].[dbo].hrSprKontragent kl (NOLOCK)
		  ON kl.id = h.KontragentID
		LEFT JOIN [RZN-OLAP].[DW].[dbo].hrSprKontragent kl2 (NOLOCK)
		  ON kl2.IDRRef = kl.GolovnojKontrag
		LEFT JOIN [RZN-OLAP].[DW].[dbo].[hrSprSklady] s (NOLOCK) 
		  ON h.SkladID = s.id
WHERE ((h.Period BETWEEN DATEADD(DAY,1,EOMONTH(GETDATE(),-1)) AND DATEADD(DAY,DAY(EOMONTH(GETDATE())),EOMONTH(GETDATE(),-1))) 
	OR (h.Period BETWEEN DATEADD(DAY,1,EOMONTH(GETDATE(),-13)) AND  DATEADD(DAY,DAY(EOMONTH(GETDATE())),EOMONTH(GETDATE(),-13))))
			AND s.Description NOT LIKE '%Транзитный%'
GROUP BY	DATEADD(DAY,1,EOMONTH(h.Period,-1)), 
			CAST(kl2.Code as nvarchar(10)), 
			n.Code, 
			h.SkladID
)

SELECT
		CAST(	CASE WHEN YEAR(Period) = YEAR(GETDATE()) 
				THEN 'ФактТекущийГод' 
				ELSE 'ФактПрошлыйГод' 
				END AS nvarchar) AS Тип, 
		t2.Department AS Подразделение, 
		t3.Direction AS Товарная_группа, 
		SUM([PosSaleCount]) AS Значение, 
		'Количество позиций продаж' AS 'Показатель' 
FROM Stroki t1 
		LEFT JOIN lg..[ClientManager] t2 
			ON t2.HeadClientCode = t1.HeadClientCode 
		LEFT JOIN lg..[Items] t3 
			ON t3.Code = t1.ItemCode 
WHERE t3.Direction IS NOT NULL AND t2.Manager IS NOT NULL AND 
		Period BETWEEN DATEADD(DAY,1,EOMONTH(DATEADD(MONTH,-1,GETDATE()))) AND DATEADD(DAY,DATEDIFF(DAY,0,GETDATE()),0) 
			OR Period BETWEEN DATEADD(DAY,1,EOMONTH(DATEADD(MONTH,-13,GETDATE()))) AND DATEADD(YEAR,-1, DATEDIFF(DAY,0,GETDATE())) AND t3.Direction IS NOT NULL
GROUP BY	t2.Department, 
			t3.Direction, 
			CASE WHEN YEAR(Period) = YEAR(GETDATE()) 
				THEN 'ФактТекущийГод' 
				ELSE 'ФактПрошлыйГод' 
			END

UNION ALL
SELECT
		CAST(	CASE WHEN YEAR(Period) = YEAR(GETDATE()) 
				THEN 'ФактТекущийГод' 
				ELSE 'ФактПрошлыйГод' 
				END AS nvarchar) AS Тип, 
		t2.Department AS Подразделение, 
		t3.Direction AS Товарная_группа, 
		SUM([NetPrice]) AS Значение, 
		'Себестоимость продаж' AS 'Показатель' 
FROM Stroki t1 
		LEFT JOIN lg..[ClientManager] t2 
			ON t2.HeadClientCode = t1.HeadClientCode 
		LEFT JOIN lg..[Items] t3 
			ON t3.Code = t1.ItemCode 
WHERE t3.Direction IS NOT NULL AND t2.Manager IS NOT NULL AND 
		Period BETWEEN DATEADD(DAY,1,EOMONTH(DATEADD(MONTH,-1,GETDATE()))) AND DATEADD(DAY,DATEDIFF(DAY,0,GETDATE()),0) 
		OR Period BETWEEN DATEADD(DAY,1,EOMONTH(DATEADD(MONTH,-13,GETDATE()))) AND DATEADD(YEAR,-1, DATEDIFF(DAY,0,GETDATE())) AND t3.Direction IS NOT NULL
GROUP BY	t2.Department, 
			t3.Direction, 
			CASE WHEN YEAR(Period) = YEAR(GETDATE()) 
				THEN 'ФактТекущийГод' 
				ELSE 'ФактПрошлыйГод' 
			END

UNION ALL
SELECT	CAST('План' AS nvarchar) AS 'Тип', 
		Department, 
		PlanType, 
		SUM(Value),
		Type AS Себестоимость 
FROM [LG].[dbo].[DepPlan] 
WHERE	Type IN ('Себестоимость') 
		AND Period = DATEADD(DAY,1,EOMONTH(DATEADD(MONTH,-1,GETDATE()))) 
GROUP BY	Department, 
			PlanType, 
			Type 

UNION ALL
SELECT	CAST('План' AS nvarchar) AS 'Тип', 
		Department, 
		PlanType, 
		SUM(Value),
		Type AS Себестоимость 
FROM [LG].[dbo].[DepPlan] 
WHERE	Type IN ('Количество позиций продаж') 
		AND Period = DATEADD(DAY,1,EOMONTH(DATEADD(MONTH,-1,GETDATE()))) 
GROUP BY	Department, 
			PlanType, 
			Type</property>
