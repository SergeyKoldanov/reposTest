                <property
                  dataType="System.String"
                  description="Команда SQL для выполнения."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT eomonth(data) as Period
, g.Производитель as Manufacturer
, s.Description as WareHouse
, k.Description as Category
, sum(nalich) / count(a.NomenklaturaID) as AvgRes
FROM [tbNalichieTovNaSkladah] a
LEFT JOIN goods g
	on a.NomenklaturaID = g.ID
LEFT JOIN vwKlassifKategoriya k
	on g.KlassifKategoriyaID = k.Id
LEFT JOIN vwOtmetka o
	on a.OtmetkaID = o.id
LEFT JOIN vwSklady s
	on s.id = a.SkladID
WHERE Data &gt;= '20210101' and data &lt; DATEADD(day,1,EOMONTH(DATEADD(month,-1, getdate())))
and o.Code IN ('А', 'В', 'С')
and s.id IN (145, 161, 133, 5, 66, 163, 156)
group by g.Производитель
, s.Description
, k.Description
, eomonth(data)

union

SELECT cast(data as date) as Period
, g.Производитель as Manufacturer
, s.Description as WareHouse
, k.Description as Category
, sum(nalich) / count(a.NomenklaturaID) as AvgRes
FROM [tbNalichieTovNaSkladah] a
LEFT JOIN goods g
	on a.NomenklaturaID = g.ID
LEFT JOIN vwKlassifKategoriya k
	on g.KlassifKategoriyaID = k.Id
LEFT JOIN vwOtmetka o
	on a.OtmetkaID = o.id
LEFT JOIN vwSklady s
	on s.id = a.SkladID
WHERE Data = cast(dateadd(d, -1, getdate()) as date)
and o.Code IN ('А', 'В', 'С')
and s.id IN (145, 161, 133, 5, 66, 163, 156)
group by g.Производитель
, s.Description
, k.Description
, cast(data as date)</property>
