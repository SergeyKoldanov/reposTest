                <property
                  dataType="System.String"
                  description="Команда SQL для выполнения."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH Nom AS (
SELECT _IDRRef, _ParentIDRRef, _Code, _Description, _Description as Родитель
FROM [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference53] WITH (NOLOCK)
WHERE _IDRRef in (0x8AEA00E098C184C111DDC30A4FF76E7A, 0xB395E4115BD714A811E2146B7D954204, 0xB395E4115BD714A811E2146B7D954205)

UNION ALL

SELECT N._IDRRef, N._ParentIDRRef, N._Code, N._Description, Nom.Родитель
FROM Nom INNER JOIN [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference53] N (NOLOCK) 
	ON Nom._IDRRef = N._ParentIDRRef
),


--контрагенты
Kontr AS (
SELECT _IDRRef, _ParentIDRRef, _Code, _Description, _Description as Родитель, _Fld638RRef
FROM [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference47] WITH (NOLOCK)
WHERE _IDRRef in (0x80C330E1715C531711EC1797339B0018
				 ,0x80C330E1715C531711EC17AAECB8A736
				 ,0x80C330E1715C531711EC17AAF48B5C55
				 ,0x80C330E1715C531711EC17AB2BD3D3B1
				 ,0x80C330E1715C531711EC17AD6B2D6D86
				 ,0x80C330E1715C531711EC24F685AE89D0
,0x80C430E1715C531711EC6242E2D54251
,0x9B34E4115BD714A811E475C5BFFB323E			,0x8AEA00E098C184C111DDC309BC6AC8A8
,0xA714D8D385E1136A11DFF6CE0DF5BB76	
				 )

UNION ALL

SELECT K._IDRRef, K._ParentIDRRef, K._Code, K._Description, Kontr.Родитель, K._Fld638RRef
FROM Kontr INNER JOIN [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference47] K (NOLOCK) 
	ON Kontr._IDRRef = K._ParentIDRRef
)

SELECT dateadd(day, 1, eomonth(P._Period, -1)) AS Period
	,S._Description AS [Sklad]
	,K._Code AS ClientCode
	,KO._Code AS HeadClientCode
	,SUM(P._Fld7753) AS Counts
	,SUM(_Fld7754) AS Price
	,SUM(_Fld7755) AS FullPrice
	,SUM(_Fld9471) AS NetPrice
	,N._Code AS ItemCode
	,M._Code AS Manager
FROM [SQL-NODE-01].[RelefUTWork].[dbo].[_AccumRg7743] P (NOLOCK) --ПРОДАЖИ	
	JOIN Kontr K --КОНТРАГЕНТЫ
		ON P._Fld7752RRef = K._IDRRef
	JOIN Kontr KO (NOLOCK) --КОНТРАГЕНТЫ ГОЛОВНЫЕ
		ON KO._IDRRef = K._Fld638RRef
	JOIN Nom N (NOLOCK) --НОМЕНКЛАТУРА
		ON P._Fld7744RRef = N._IDRRef
	LEFT JOIN [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference60] M (NOLOCK) --МЕНЕДЖЕР
		ON P._Fld19240RRef = M._IDRRef
	LEFT JOIN [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference70] S (NOLOCK) --СКЛАДЫ
		ON P._Fld10714RRef = S._IDRRef
WHERE P._Period &gt;= DATEADD(DAY,1,EOMONTH(getdate(),-4)) AND P._Period &lt;= DATEADD(hh, 7, CAST(FLOOR(CAST(GETDATE() as decimal(38,19))) as datetime))
GROUP BY dateadd(day, 1, eomonth(P._Period, -1))
	,S._Description
	,K._Code
	,KO._Code
	,N._Code
	,M._Code</property>
