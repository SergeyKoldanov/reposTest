                <property
                  dataType="System.String"
                  description="Указывает имя объекта базы данных, используемого для открытия набора строк."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Указывает переменную, содержащую имя объекта базы данных, используемого для открытия набора строк."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="Команда SQL для выполнения."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">with Kontr AS (
SELECT _IDRRef, _ParentIDRRef, _Code, _Description, _Description as Родитель, _Fld638RRef, _Fld651RRef, _Fld660RRef
  , CASE
	WHEN _Code = 154527 THEN 'B2B'
	WHEN _Code = 156168 THEN 'Дистрибьюция'
	WHEN _Code = 158772 THEN 'Индастри'
	WHEN _Code = 156688 THEN 'Ключевые клиенты'
	WHEN _Code = 156173 THEN 'Сети'
	WHEN _Code = 156171 THEN 'Художественный'
	WHEN _Code = 156169 THEN 'Экспорт'
	WHEN _Code = 090564 THEN 'Индастри'
	WHEN _Code = 003647 THEN 'Корпоративный Департамент'
	WHEN _Code = 004361 or _Code = 041825 THEN 'Канцлеры'
	END AS Direction
FROM [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference47] WITH (NOLOCK)
WHERE _IDRRef in (0x80C330E1715C531711EC1797339B0018--В2В
, 0x80C330E1715C531711EC17AAECB8A736--Дистрибьюция
, 0x8AEA00E098C184C111DDC309BC6AC8A8--КД
, 0x80C330E1715C531711EC17AD6B2D6D86--Сети
, 0x80C330E1715C531711EC17AB2BD3D3B1--ХД
, 0x80C330E1715C531711EC17AAF48B5C55--Экспорт
, 0x8AEA00E098C184C111DDC309C28B91BB--Филиалы
, 0x80C330E1715C531711EC24F685AE89D0--Ключевые
, 0x80C430E1715C531711EC6242E2D54251--Индастри
, 0xA714D8D385E1136A11DFF6CE0DF5BB76--канцлеры
)--свт индастри 

UNION ALL

SELECT K._IDRRef, K._ParentIDRRef, K._Code, K._Description, Kontr.Родитель, K._Fld638RRef, K._Fld651RRef, K._Fld660RRef, Kontr.Direction
FROM Kontr INNER JOIN [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference47] K (NOLOCK) 
	ON Kontr._IDRRef = K._ParentIDRRef
)


SELECT distinct K._Code			as ГоловнойКонтрагентКод
	,K._Description		as ГоловнойКонтрагентНаименование
	,KO._Code			as Код
	,KO._Description	as Наименование
	,M._Description		as ГоловнойКонтрагентОсновнойМенеджерПокупателяНаименование
	,F._Description		as ГоловнойКонтрагентОсновнойМенеджерПокупателяФизЛицоНаименование
	,P._Description		as ГоловнойКонтрагентОсновнойМенеджерПокупателяФизЛицорцЦФОНаименование
	,cast(k.Direction as nvarchar) as Direction
	--,T._Description		as ЗначениеНаименование
	,R._Description		as Регион
	,TT.Del				as Отсрочка
	,TT.Limit			as Сумма
FROM [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference47] KO (NOLOCK)  
	 INNER JOIN Kontr K (NOLOCK)
		ON K._IDRRef = KO._Fld638RRef
	 LEFT JOIN [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference60] M (NOLOCK)
		ON K._Fld651RRef = M._IDRRef
	 LEFT JOIN [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference83] F (NOLOCK)
		ON M._Fld910RRef = F._IDRRef
	 LEFT JOIN [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference59] P (NOLOCK)
		ON F._Fld9336RRef = P._IDRRef
	 LEFT JOIN [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference66] R (NOLOCK)
		ON K._Fld660RRef = R._IDRRef
	 /*LEFT JOIN (SELECT SS._Description
					,S._Fld25410RRef
				FROM [SQL-NODE-01].[RelefUTWork].[dbo].[_InfoRg25409] S (NOLOCK)
					JOIN[SQL-NODE-01].[RelefUTWork].[dbo].[_Reference25400] SS (NOLOCK)
						ON S._Fld25414_RRRef = SS._IDRRef
					LEFT JOIN[SQL-NODE-01].[RelefUTWork].[dbo].[_Chrc25401] C (NOLOCK)
						ON S._Fld25413RRef = C._IDRRef
				WHERE C._Code = 000000001
                                                                                             AND S._Fld25411RRef = 0) --дублировались строки, т.к. попадали клиенты с двумя торговыми точками
                                                                                                                                                      T
		ON K._IDRRef = T._Fld25410RRef*/
	 LEFT JOIN (SELECT I._Fld9187RRef AS Client
					,I._Fld9189 AS Limit
					,I._Fld9190 AS Del
			    FROM [SQL-NODE-01].[RelefUTWork].[dbo].[_InfoRg9186] I (NOLOCK)
					LEFT JOIN [SQL-NODE-01].[RelefUTWork].[dbo].[_Enum370] E (NOLOCK)
						ON I._Fld9188RRef = E._IDRRef
			    WHERE E._EnumOrder = 0
					AND I._Period = (SELECT MAX(L._Period)
								     FROM [SQL-NODE-01].[RelefUTWork].[dbo].[_InfoRg9186] L (NOLOCK)
								     WHERE I._Fld9187RRef = L._Fld9187RRef)) AS TT
		ON K._IDRRef = TT.Client
WHERE K._Code = KO._Code</property>
