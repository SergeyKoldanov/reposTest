			LEFT JOIN [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference70] as S WITH (NOLOCK) -- SKLAD
				ON S._IDRRef = T1._Fld7919RRef
		WHERE T1._Period BETWEEN DATEADD(DAY,1,EOMONTH(GETDATE(),-1)) AND GETDATE()
			AND  N._Code IN ('251998', '314473', '321552', '315140', '315458', '320247', '317325',
