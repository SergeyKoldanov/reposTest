			LEFT JOIN [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference53] as N WITH (NOLOCK) -- номенклатура
				ON T1._Fld7920RRef = N._IDRRef
			LEFT JOIN [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference47] as K WITH (NOLOCK) -- контрагент 
