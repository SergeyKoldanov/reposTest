                    <property
                      dataType="System.String"
                      description="Команда SQL для выполнения."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @Start DATETIME
DECLARE @end DATETIME

SET @Start = '2008-03-01 11:15:41.730'
SET @end = '2028-03-02 11:15:41.730'

SELECT
  pr.NameRU COLLATE Cyrillic_General_CI_AS AS [Сотрудник]
 ,cast(kc.people COLLATE Cyrillic_General_CI_AS as nvarchar) AS [ФИО]
 ,cast(br.RoleName COLLATE Cyrillic_General_CI_AS as nvarchar) AS [Отдел]
 ,CAST(a.RecordDate AS DATE) Data
 ,[Час] = DATEPART(HH, a.RecordDate)
 ,[Товар] = d.tasteen COLLATE Cyrillic_General_CI_AS
 ,[Наименование] = d.NameRU COLLATE Cyrillic_General_CI_AS
 ,[Кол-во в БЕ] = SUM(a.Quantity)
 ,FLOOR(SUM(a.Quantity * c.NettoWeight)) AS [Вес]
 ,SUM(a.Quantity * c.UnitVolume) AS [Объем]
 ,[Вид откланения] = ISNULL(dc.NameRU COLLATE Cyrillic_General_CI_AS , '--')
 ,[Транспорт] = wi.DocumentNumber COLLATE Cyrillic_General_CI_AS
 ,[Документы] = ISNULL(vl.VisitorRequestNames, wi.DocumentNumber) COLLATE Cyrillic_General_CI_AS
 ,[#] = wi.tid
FROM hdr_WarehouseIncome wi WITH (NOLOCK)
JOIN Transactions tr WITH (NOLOCK)
  ON tr.tid = wi.Transaction_id
LEFT JOIN VisitorsLog vl WITH (NOLOCK)
  ON wi.Transport_id = vl.tid
JOIN tbl_WarehouseIncomeMaterials a WITH (NOLOCK)
  ON a.Transaction_id = wi.Transaction_id
JOIN MaterialUnits AS c WITH (NOLOCK)
  ON a.MaterialUnit_id = c.tid
JOIN Materials AS d WITH (NOLOCK)
  ON a.Material_id = d.tid
LEFT JOIN BarcodeObjects b WITH (NOLOCK)
  ON a.BarcodeObject_id = b.tid
LEFT JOIN DiscountCategories dc WITH (NOLOCK)
  ON b.DiscountCategory_id = dc.tid
LEFT JOIN ProductionResources pr WITH (NOLOCK)
  ON pr.tid = a.ProductionResource_id
LEFT JOIN kiscodePR kcpr WITH (NOLOCK)
  ON kcpr.tidpr = pr.tid
LEFT JOIN kiscode kc WITH (NOLOCK)
  ON kc.tid = kcpr.kiscode_id
LEFT JOIN PeopleProductionResources ppr WITH (NOLOCK)
  ON pr.tid = ppr.ProductionResource_id
LEFT JOIN users u WITH (NOLOCK)
  ON ppr.People_id = u.People_id
LEFT JOIN UserRoles ur WITH (NOLOCK)
  ON u.tid = ur.User_id
LEFT JOIN BusinessRoles br WITH (NOLOCK)
  ON br.tid = ur.Role_id
WHERE a.RecordDate &lt;= @end
AND a.RecordDate &gt;= @Start
GROUP BY pr.NameRU
        ,CAST(a.RecordDate AS DATE)
        ,DATEPART(HH, a.RecordDate)
        ,ISNULL(vl.VisitorRequestNames, wi.DocumentNumber)
        ,wi.tid
        ,wi.DocumentNumber
        ,d.tasteen
        ,d.NameRU
        ,dc.NameRU
		,kc.People
		,br.RoleName</property>
