                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @dn DATETIME
SET @dn = CAST(ROUND(CAST(GETDATE() AS FLOAT) ,0 ,1) AS DATETIME)- 3
--SET @dn = '20160101'

SELECT DISTINCT
       [Начало исполнения] = a.TaskDate
      ,[Конец исполнения]     = a.FinishDate
      ,[ОС]                   = a.StorageObject_id
      ,[Из ТЗ]                = tz.NameRU COLLATE Cyrillic_General_CI_AS
      ,[Из ячейки]            = lo.locationname COLLATE Cyrillic_General_CI_AS
      ,[В ТЗ]                 = tz1.nameru COLLATE Cyrillic_General_CI_AS
      ,[В ячейку]             = lo1.LocationName COLLATE Cyrillic_General_CI_AS
      ,[Ресурс]               = pr.NameRU COLLATE Cyrillic_General_CI_AS
      ,[ГПР]                  = prg.NameRU COLLATE Cyrillic_General_CI_AS
      ,[Объем]                = ABS(aa.VolumePACK)
      ,ABS(aa.Weight) AS [Вес]
      ,[Товаропоток] = ISNULL(a.MovementFlowName ,'Вывоз товара по заказу') COLLATE Cyrillic_General_CI_AS
FROM   hdr_ObjectMovement a WITH (NOLOCK)
       JOIN locations lo WITH (NOLOCK)
            ON  lo.tid = a.FromLocation_id
       JOIN technozones tz WITH (NOLOCK)
            ON  tz.tid = lo.TechnoZone_id
       JOIN locations lo1 WITH (NOLOCK)
            ON  lo1.tid = a.ToLocation_id
       JOIN technozones tz1 WITH (NOLOCK)
            ON  tz1.tid = lo1.TechnoZone_id
       LEFT JOIN ProductionResources pr WITH (NOLOCK)
            ON  pr.tid = a.ProductionResource_id
       LEFT JOIN ProductionResourceGroups prg WITH (NOLOCK)
            ON  prg.tid = pr.ProductionResourceGroup_id
       LEFT JOIN storageobjects so WITH (NOLOCK)
            ON  so.tid = a.StorageObject_id
       OUTER APPLY (
    SELECT mpr.StorageObject_id
          ,mpr.ToLocation_ID
          ,mpr.VolumePACK
          ,mpr.Weight
          ,mpr.Quantity AS SOMPR
    FROM   MovementMPR mpr WITH (NOLOCK)
    WHERE  mpr.StorageObject_ID = a.StorageObject_id
           AND mpr.ToLocation_ID = a.FromLocation_id
) aa

LEFT JOIN ZKO zko WITH (NOLOCK)
            ON  zko.storageobject_id = a.StorageObject_id
WHERE  prg.NameRU LIKE '%ПРТ%'
       AND a.TaskDate&gt;= @dn</property>
