                    <property
                      dataType="System.String"
                      description="Команда SQL для выполнения."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SET DATEFORMAT mdy
DECLARE @date1 DATETIME
DECLARE @date2 DATETIME
SET @date1 = CAST(ROUND(CAST(GETDATE() AS FLOAT), 0, 1) AS DATETIME) - 3

--SET @date1 = '10-18-2010 0:00:00' 

SET @date2 = '10-18-2030 0:00:00'

SELECT
 kc.face COLLATE Cyrillic_General_CI_AS AS 'Кто комплектовал'
,b1.ShortName COLLATE Cyrillic_General_CI_AS AS ShortName
,e.WayListNumber COLLATE Cyrillic_General_CI_AS AS 'Путевой лист'
,c.ExternalCode COLLATE Cyrillic_General_CI_AS AS 'Заказ'
--,COUNT(a.StorageObject_id) - ISNULL(SUM(g.sbor), 0) AS 'Кол-во фабричных коробов'
,case when su.NameRU like '%Короб%' then COUNT(a.StorageObject_id) - ISNULL(SUM(g.sbor), 0)
      else '' end AS 'Кол-во фабричных коробов'

,ISNULL(SUM(g.sbor), 0) AS 'Кол-во сборных коробов'
,a.Recorddate
,a.BoxVolume
,a.BoxWeight
,[CountMasterOS] = ISNULL(a.Bottom, a.SourceObject_id)
,a.FullPicking COLLATE Cyrillic_General_CI_AS AS [Признак Моно-ОС]
,bs.RoleName COLLATE Cyrillic_General_CI_AS AS [Отдел]
,SUBSTRING (spc.Description, 0, 5) COLLATE Cyrillic_General_CI_AS as [Код спецобработки]

FROM DefragLog AS a WITH (NOLOCK)
join StorageObjects so with(nolock) on so.tid = a.StorageObject_id
join StorageUnits su with(nolock) on su.tid = so.StorageUnit_id

JOIN Transactions AS b WITH (NOLOCK) ON a.DeliveryTransaction_id = b.tid
JOIN hdr_DeliveryRequest AS c WITH (NOLOCK) ON b.ParentTransaction_id = c.Transaction_id
JOIN TransportRequests AS d WITH (NOLOCK) ON b.ParentTransaction_id = d.RequestTransaction_id
JOIN VisitorsLog AS e WITH (NOLOCK) ON d.Visitor_id = e.tid
JOIN [LEAD].dbo.ProductionResources b1 WITH (NOLOCK) ON b1.tid = a.ProductionResource_id

OUTER APPLY (SELECT 1 AS sbor, b1.Wave_id,st.NameRU
             FROM tbl_WaveTare AS b1 WITH (NOLOCK)
			 join Waves w with(nolock) on w.tid = b1.Wave_id
             join WaveTypes wt with(nolock) on wt.tid = w.WaveType_id
			 join StorageUnits st with(nolock) on st.tid = b1.StorageUnit_id
             WHERE a.StorageObject_id = b1.StorageObject_id and st.NameRU like '%Короб%'
			       and (wt.WaveName not like '%6в1%' and wt.WaveName not like '%4в1%')) AS g

LEFT JOIN PeopleProductionResources ppl WITH (NOLOCK) ON ppl.ProductionResource_id = b1.tid
LEFT JOIN people pl WITH (NOLOCK) ON pl.tid = ppl.People_id
LEFT JOIN Users us WITH (NOLOCK) ON us.People_id = pl.tid
LEFT JOIN [LEAD].dbo.UserRoles ur WITH (NOLOCK) ON ur.User_id = us.tid
LEFT JOIN [LEAD].dbo.BusinessRoles bs WITH (NOLOCK) ON bs.tid = ur.Role_id
LEFT JOIN KisCodePR kc WITH (NOLOCK) ON kc.TidPR = b1.tid
LEFT JOIN SpecialProcessCodes spc WITH (NOLOCK) on spc.tid = c.SpecialProcessCode_id

WHERE a.Recorddate &gt;= @date1
AND a.Recorddate &lt;= @date2
GROUP BY kc.face
        ,e.WayListNumber
        ,c.ExternalCode
        ,a.Recorddate
        ,a.tid
        ,a.BoxVolume
        ,a.BoxWeight
        ,ISNULL(a.Bottom, a.SourceObject_id)
        ,a.FullPicking
        ,b1.ShortName
        ,bs.RoleName
        ,spc.Description, su.NameRU</property>
