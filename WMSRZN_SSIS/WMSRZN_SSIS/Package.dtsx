                    <property
                      dataType="System.String"
                      description="Команда SQL для выполнения."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SET DATEFORMAT mdy
DECLARE @date1 DATETIME
DECLARE @date2 DATETIME
--SET @date1 = '2021-01-01 00:00:00.297'
SET @date2 = '2031-12-30 00:00:00.297'

SET @date1 = (CAST(round(CAST(GETDATE() as float),0,1) as datetime) - 3)


SELECT DISTINCT
  CAST(a.TaskDate AS DATE) Data
 ,DATEPART(HOUR, a.taskdate) AS [Час]
 ,a.StorageObject_id AS [ОС]
 ,ABS(aa.VolumePACK) AS [Объем]
 ,ABS(aa.Weight) AS [Вес]

 ,tz.NameRU COLLATE Cyrillic_General_CI_AS AS [ИЗ ТЗ]
 ,lo.LocationName COLLATE Cyrillic_General_CI_AS AS [ИЗ ячейки]
 ,tz1.NameRU COLLATE Cyrillic_General_CI_AS AS [В ТЗ]
 ,lo1.LocationName COLLATE Cyrillic_General_CI_AS AS [В Ячейку]
 ,nz.NameRu COLLATE Cyrillic_General_CI_AS AS [Блок]
 ,a.MovementFlowName COLLATE Cyrillic_General_CI_AS AS [Поток]

 ,kc.Face COLLATE Cyrillic_General_CI_AS AS [Лицо со Шрамом(С)]
 ,b.ShortName COLLATE Cyrillic_General_CI_AS AS [Сотрудник]
 ,CASE
    WHEN a.MovementFlowName  = 'Пополнение зоны пикинга' AND
      IsPlaned = 1 THEN 'ХП'
    WHEN a.MovementFlowName  = 'Пополнение зоны пикинга' AND
      IsPlaned = 0 THEN 'ГП'
    ELSE ''
  END COLLATE Cyrillic_General_CI_AS AS[Тип пополнения]
 ,bs.RoleName COLLATE Cyrillic_General_CI_AS AS [Роль] 
FROM hdr_ObjectMovement a
OUTER APPLY (SELECT TOP 1
    a1.VolumePACK
   ,a1.Weight
  FROM MovementMPR a1
  WHERE a.StorageObject_ID = a1.StorageObject_ID) aa
LEFT JOIN locations lo WITH (NOLOCK)
  ON lo.tid = a.FromLocation_id
LEFT JOIN locations lo1 WITH (NOLOCK)
  ON lo1.tid = a.ToLocation_id
LEFT JOIN technozones tz WITH (NOLOCK)
  ON tz.tid = lo.TechnoZone_id
LEFT JOIN technozones tz1 WITH (NOLOCK)
  ON tz1.tid = lo1.TechnoZone_id
LEFT JOIN ProductionResources b WITH (NOLOCK)
  ON b.tid = a.ProductionResource_id
LEFT JOIN PeopleProductionResources ppl WITH (NOLOCK)
  ON ppl.ProductionResource_id = b.tid
LEFT JOIN people pl WITH (NOLOCK)
  ON pl.tid = ppl.People_id
LEFT JOIN Users us WITH (NOLOCK)
  ON us.People_id = pl.tid
LEFT JOIN [LEAD].dbo.UserRoles ur WITH (NOLOCK)
  ON ur.User_id = us.tid
LEFT JOIN [LEAD].dbo.BusinessRoles bs WITH (NOLOCK)
  ON bs.tid = ur.Role_id

LEFT JOIN KisCodePR kc WITH (NOLOCK)
  ON kc.TidPR = b.tid
LEFT JOIN NarrowZones nz WITH (NOLOCK)
  ON nz.tid = lo.NarrowZone_id

WHERE a.TaskDate &gt;= @date1
AND a.TaskDate &lt;= @date2</property>
