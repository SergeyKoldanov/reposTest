                    <property
                      dataType="System.String"
                      description="Команда SQL для выполнения."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Declare @Start datetime 
Declare @End datetime

set @Start='2016-09-01 00:00:00.000'
set @End='2029-10-14 00:00:00.000'

set @Start= (CAST(round(CAST(GETDATE() as float),0,1) as datetime) - 3)

SELECT 


[Операция]='Перемещения' COLLATE Cyrillic_General_CI_AS,
count(a.tid)   [Кол] 

,datepart(year,a.FinishDate)   [Год]
,datepart( month,a.FinishDate)   [Месяц]
,datepart(day,a.FinishDate)   [День]
,datepart(hour,a.FinishDate)   [Час]

,mf.NameRU COLLATE Cyrillic_General_CI_AS    [ТП]
,kc.Face COLLATE Cyrillic_General_CI_as    [Физ.лицо]
,c.NameRU COLLATE Cyrillic_General_CI_AS    [ПР]
,tz.nameru COLLATE Cyrillic_General_CI_AS    [ТЗ из]
,bs.rolename COLLATE Cyrillic_General_CI_AS    [Отдел]
,a.FinishDate [Дата]
FROM [LEAD].dbo.hdr_ObjectMovement   a with (nolock)  
JOIN [LEAD].dbo.Transactions   b with (nolock) ON a.Transaction_id = b.tid   
LEFT JOIN [LEAD].dbo.ProductionResources   c with (nolock) ON a.ProductionResource_id = c.tid
left join [LEAD].dbo.MovementFlows   mf with (nolock)on isnull(a.MovementFlow_id,5) = mf.tid  
left join [LEAD].dbo.PeopleProductionResources ppl with (nolock) on ppl.ProductionResource_id=c.tid
left join [LEAD].dbo.people pl with (nolock) on pl.tid=ppl.People_id
left join [LEAD].dbo.Users us with (nolock) on us.People_id=pl.tid
left join [LEAD].dbo.UserRoles ur with (nolock) on ur.User_id=us.tid
left join [LEAD].dbo.BusinessRoles bs with (nolock) on bs.tid=ur.Role_id
left join [LEAD].dbo.KisCodePR kc with (nolock) on kc.TidPR=c.tid
left join [LEAD].dbo.Locations ss with (nolock) on ss.tid=a.fromlocation_id
left join [LEAD].dbo.technozones tz with (nolock) on tz.tid=ss.technozone_id

WHERE
(b.TransactionStatus = 1 )
and a.FinishDate&gt;=@Start and a.FinishDate&lt;=@End

group by FinishDate ,c.nameru,mf.NameRU,kc.Face
,tz.nameru
,bs.rolename</property>
