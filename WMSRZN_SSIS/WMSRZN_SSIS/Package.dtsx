                    <property
                      dataType="System.String"
                      description="Команда SQL для выполнения."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT
  [Cотрудник] = b.nameru COLLATE Cyrillic_General_CI_AS
 ,[Дата] = CAST(c.DateStart AS DATE )
 ,[Час] = CONVERT(VARCHAR(15), c.DateStart, 108) COLLATE Cyrillic_General_CI_AS
 ,[Товар] = f.TasteEN COLLATE Cyrillic_General_CI_AS
 ,[Наименование] = f.NameRU COLLATE Cyrillic_General_CI_AS
 ,[Кол-во в БЕ] = e.Quantity
 ,[Вес] = g.NettoWeight * e.Quantity
 ,[Объем] = (g.Length * g.Width * g.Height) * e.Quantity
 ,[Транспорт] = i.RouteNumber COLLATE Cyrillic_General_CI_AS
 ,[Документы] = cast(d.ExternalCode as varchar(100)) COLLATE Cyrillic_General_CI_AS
FROM Hdr_DeliveryIncomeCross AS c WITH (NOLOCK)
JOIN productionresources AS b WITH (NOLOCK)
  ON c.ProductionResource_id = b.tid
JOIN hdr_DeliveryRequest AS d WITH (NOLOCK)
  ON c.DeliveryTransaction_id = d.Transaction_id
JOIN tbl_DeliveryRequestMaterials AS e WITH (NOLOCK)
  ON e.Transaction_id = d.Transaction_id
JOIN materials AS f WITH (NOLOCK)
  ON f.tid = e.Material_id
JOIN MaterialUnits AS g WITH (NOLOCK)
  ON g.tid = e.MaterialUnit_id
JOIN TransportRequests AS h WITH (NOLOCK)
  ON h.RequestTransaction_id = d.Transaction_id
JOIN VisitorsLog AS i WITH (NOLOCK)
  ON i.tid = h.Visitor_id
WHERE d.IsCross = 1</property>
