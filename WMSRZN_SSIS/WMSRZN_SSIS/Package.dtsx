                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="Команда SQL для выполнения."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT
       aa.[Макс.дата]
      ,vl.WayListNumber COLLATE Cyrillic_General_CI_AS AS [ПЛ]
      ,dr.PurchaseNumber COLLATE Cyrillic_General_CI_AS AS [Заявка]
      ,aa.[Кол-во сборных мест]
      ,aa.[Кол-во фабричных мест]
      ,aa.[Кол-во мест КД]
      ,drr.Transaction_id
      ,kc.face COLLATE Cyrillic_General_CI_AS AS [Физ. лицо]
      ,drr.DebtorPartnerName COLLATE Cyrillic_General_CI_AS AS [Партнер]
      ,bs.RoleName COLLATE Cyrillic_General_CI_AS AS [Отдел]
      ,SUBSTRING(spc.Description ,0 ,5) COLLATE Cyrillic_General_CI_AS AS [Код спецобработки]
      ,DATEPART(hh ,aa.[Макс.дата]) [Час]
FROM   ShipmentVisitorLog a WITH (NOLOCK)
       JOIN VisitorsLog vl WITH (NOLOCK)
            ON  vl.tid = a.Visitor_id
       JOIN hdr_Delivery dr WITH (NOLOCK)
            ON  dr.Transaction_id = a.DeliveryTransaction_id
       JOIN ProductionResources pr WITH (NOLOCK)
            ON  pr.tid = a.ProductionResource_id
       JOIN transactions tr WITH (NOLOCK)
            ON  tr.tid = dr.Transaction_id
       JOIN hdr_DeliveryRequest drr WITH (NOLOCK)
            ON  drr.Transaction_id = tr.ParentTransaction_id
       LEFT JOIN Gates g WITH (NOLOCK)
            ON  g.tid = vl.Gate_id
       LEFT JOIN SpecialProcessCodes spc WITH (NOLOCK)
            ON  spc.tid = drr.SpecialProcessCode_id
       LEFT JOIN PeopleProductionResources ppl WITH (NOLOCK)
            ON  ppl.ProductionResource_id = pr.tid
       LEFT JOIN people pl WITH (NOLOCK)
            ON  pl.tid = ppl.People_id
       LEFT JOIN Users us WITH (NOLOCK)
            ON  us.People_id = pl.tid
       LEFT JOIN UserRoles ur WITH (NOLOCK)
            ON  ur.User_id = us.tid
       LEFT JOIN BusinessRoles bs WITH (NOLOCK)
            ON  bs.tid = ur.Role_id
       LEFT JOIN KisCodePR kc WITH (NOLOCK)
            ON  kc.TidPR = pr.tid
       LEFT JOIN (
                SELECT DISTINCT
                       a.LocalValue
                      ,MAX(bb.RecordDate) AS [Макс.дата]
                      ,SUM(bb.MadeCargoPlacesShipped) AS [Кол-во сборных мест]
                      ,SUM(bb.TotalCargoPlacesShipped- bb.MadeCargoPlacesShipped) AS [Кол-во фабричных мест]
                      ,SUM(bb.CargoPlaces) AS [Кол-во мест КД]
                      ,[Статус] = CASE 
                                       WHEN bb.DeliveryStatus='CANCELED' THEN 'CANCELED'
                                       WHEN bb.DeliveryStatus='PICKED' THEN 'PICKED'
                                       ELSE 'SHIPPED'
                                  END
                FROM   temp_ElementIntegration_623 a
                       JOIN TRANSIT..hdr_DeliveryResponse bb
                            ON  bb.ExternalCode = a.TargetValue
                WHERE  bb.DeliveryStatus = 'SHIPPED'
                GROUP BY
                       a.LocalValue
                      ,bb.DeliveryStatus
            ) aa
            ON  aa.LocalValue = drr.Transaction_id
                
                --ORDER BY aa.[Макс.дата] DESC</property>
