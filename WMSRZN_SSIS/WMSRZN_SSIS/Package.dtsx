          DTS:LocaleID="-1"
          DTS:ObjectName="Задача потока данных 2 1 1"
          DTS:TaskContact="Performs high-performance data extraction, transformation and loading;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1">
          <DTS:Variables />
          <DTS:ObjectData>
            <pipeline
              version="1">
              <components>
                <component
                  refId="Package\Контейнер последовательности\Задача потока данных 2 1 1\Источник &quot;OLE DB&quot;"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="Источник &quot;OLE DB&quot;;Корпорация Майкрософт. Microsoft SQL Server. (C) Корпорация Майкрософт (Microsoft Corporation). Все права защищены. http://www.microsoft.com/sql/support;7"
                  description="Источник &quot;OLE DB&quot;"
                  name="Источник &quot;OLE DB&quot;"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="Время ожидания команды, в секундах. Величина 0 означает неограниченное время ожидания."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Указывает имя объекта базы данных, используемого для открытия набора строк."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Указывает переменную, содержащую имя объекта базы данных, используемого для открытия набора строк."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="Команда SQL для выполнения."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @date1 DATETIME
DECLARE @date2 DATETIME
SET @date1 = CAST(ROUND(CAST(GETDATE() AS FLOAT), 0, 1) AS DATETIME) - 3

--SET @date1 = '20210901'
SET @date2 = '20280101'

SELECT aa.Операция COLLATE Cyrillic_General_CI_AS AS Операция
      ,aa.[Кол-во SKU]
      ,aa.[Кол-во]
      ,[Единица] = CASE 
                        WHEN aa.Единица=3 THEN 'Шт'
                        ELSE     'Упак'
                   END COLLATE Cyrillic_General_CI_AS
      ,aa.[кол-во строк]
      ,aa.Отдел COLLATE Cyrillic_General_CI_AS AS Отдел
      ,aa.[Физ. лизцо] COLLATE Cyrillic_General_CI_AS AS [Пользователь]
      ,aa.Дата
      ,aa.Час
      ,aa.Вес
      ,aa.Объем
      ,aa.WaveName COLLATE Cyrillic_General_CI_AS WaveName
      ,aa.NameRU COLLATE Cyrillic_General_CI_AS NameRU
FROM   (
           SELECT [Операция] = 'Набор кладовщиками' COLLATE Cyrillic_General_CI_AS
                 ,[Кол-во SKU]     = a.Material_id
                 ,[Кол-во]         = FLOOR(
                      SUM(
                          CASE 
                               WHEN b1.UnitKoeff&gt;0 THEN a.BaseQuantity/b1.UnitKoeff
                               ELSE 0
                          END
                      )
                  )
                 ,[Отдел]          = bs.RoleName COLLATE Cyrillic_General_CI_AS
                 ,b.NameRU COLLATE Cyrillic_General_CI_AS AS [Пользователь]
                 ,CAST(a.TaskDate AS DATE) AS [Дата]
                 ,DATEPART(HOUR ,a.TaskDate) AS [Час]
                 ,COUNT(a.tid)   AS [кол-во строк]
                 ,[Вес] = SUM(
                      (
                          CASE 
                               WHEN mu.UnitKoeff&gt;0 THEN a.BaseQuantity/mu.UnitKoeff
                               ELSE 0
                          END
                      )*mu.BruttoWeight
                  )
                 ,[Объем] = SUM(
                      (
                          CASE 
                               WHEN mu.UnitKoeff&gt;0 THEN a.BaseQuantity/mu.UnitKoeff
                               ELSE 0
                          END
                      )*mu.UnitVolume
                  )
                 ,z.WaveName COLLATE Cyrillic_General_CI_AS WaveName
                 ,tz.NameRU COLLATE Cyrillic_General_CI_AS NameRU
                 ,ck.people        AS [Физ. лизцо]
                 ,a.PackType_id  AS [Единица]
           FROM   [LEAD].dbo.hdr_MaterialPicking AS a(NOLOCK)
                  JOIN [LEAD].dbo.ProductionResources b
                       ON  b.tid = a.ProductionResource_id
                  JOIN [LEAD].dbo.tbl_WaveTare wt
                       ON  wt.tid = a.WaveTare_id
                  LEFT JOIN [LEAD].dbo.Waves AS j WITH (NOLOCK)
                       ON  a.Wave_id = j.tid
                  LEFT JOIN [LEAD].dbo.WaveTypes AS z WITH (NOLOCK)
                       ON  j.WaveType_id = z.tid
                  LEFT JOIN [LEAD].dbo.Materials ma WITH (NOLOCK)
                       ON  ma.tid = a.Material_id
                  LEFT JOIN [LEAD].dbo.MaterialUnits mu WITH (NOLOCK)
                       ON  a.AlternateMaterialUnit_id = mu.tid
                  LEFT JOIN [LEAD].dbo.Locations lo WITH (NOLOCK)
                       ON  lo.LocationName = a.SourceLocationName
                  LEFT JOIN [LEAD].dbo.TechnoZones tz
                       ON  tz.tid = lo.TechnoZone_id
                  LEFT JOIN PeopleProductionResources ppl WITH (NOLOCK) ON  ppl.ProductionResource_id = b.tid 
                                                                 LEFT JOIN people pl WITH (NOLOCK) ON  pl.tid = ppl.People_id 
                                                                 LEFT JOIN Users us WITH (NOLOCK) ON  us.People_id = pl.tid 
                                                                 LEFT JOIN [LEAD].dbo.UserRoles ur WITH (NOLOCK) ON  ur.User_id = us.tid 
                                                                 LEFT JOIN [LEAD].dbo.BusinessRoles bs WITH (NOLOCK) ON  bs.tid = ur.Role_id
                  LEFT JOIN MaterialUnits AS b1 WITH (NOLOCK)
                       ON  a.AlternateMaterialUnit_id = b1.tid LEFT JOIN KisCodePR kc WITH (NOLOCK) ON  kc.TidPR = b.tid left join KisCode ck with(nolock) on ck.tid=kc.kiscode_id
                  LEFT JOIN [LEAD].dbo.Transactions AS e WITH (NOLOCK)
                       ON  a.Transaction_id = e.tid
                  LEFT JOIN [LEAD].dbo.hdr_Delivery AS k WITH (NOLOCK)
                       ON  e.ParentTransaction_id = k.Transaction_id
           WHERE  z.WaveName NOT LIKE '%Поп%'
                  AND z.WaveName NOT LIKE '%Комис%'
                  AND z.WaveName NOT LIKE '%ЗПХ%'
                  AND a.TaskDate&lt;= @date2
                  AND a.TaskDate&gt;= @date1
           GROUP BY
                  CAST(a.TaskDate AS DATE)
                 ,b.NameRU
                 ,DATEPART(HOUR ,a.TaskDate)
                 ,z.WaveName
                 ,tz.NameRU
                 ,bs.RoleName
                 ,ck.people
                 ,a.PackType_id
                           ,a.Material_id
           UNION
           ALL
           SELECT [Операция] = 'Холодные пополнения кладовщиками '
                 ,[Кол-во SKU]     = a.Material_id
                 ,[Кол-во]         = FLOOR(
                      SUM(
                          CASE 
                               WHEN b1.UnitKoeff&gt;0 THEN a.BaseQuantity/b1.UnitKoeff
                               ELSE 0
                          END
                      )
                  )
                 ,[Отдел]          = bs.RoleName
                 ,b.NameRU       AS [Пользователь]
                 ,CAST(a.TaskDate AS DATE) AS [Дата]
                 ,DATEPART(HOUR ,a.TaskDate) AS [Час]
                 ,COUNT(a.tid)   AS [кол-во строк]
                 ,[Вес] = SUM(
                      (
                          CASE 
                               WHEN mu.UnitKoeff&gt;0 THEN a.BaseQuantity/mu.UnitKoeff
                               ELSE 0
                          END
                      )*mu.BruttoWeight
                  )
                 ,[Объем] = SUM(
                      (
                          CASE 
                               WHEN mu.UnitKoeff&gt;0 THEN a.BaseQuantity/mu.UnitKoeff
                               ELSE 0
                          END
                      )*mu.UnitVolume
                  )
                 ,z.WaveName
                 ,tz.NameRU
                 ,ck.people        AS [Физ. лизцо]
                 ,a.PackType_id  AS [Единица]
           FROM   [LEAD].dbo.hdr_MaterialPicking AS a(NOLOCK)
                  JOIN [LEAD].dbo.ProductionResources b
                       ON  b.tid = a.ProductionResource_id
                  JOIN [LEAD].dbo.tbl_WaveTare wt
                       ON  wt.tid = a.WaveTare_id
                  LEFT JOIN [LEAD].dbo.Waves AS j WITH (NOLOCK)
                       ON  a.Wave_id = j.tid
                  LEFT JOIN [LEAD].dbo.WaveTypes AS z WITH (NOLOCK)
                       ON  j.WaveType_id = z.tid
                  LEFT JOIN [LEAD].dbo.Materials ma WITH (NOLOCK)
                       ON  ma.tid = a.Material_id
                  LEFT JOIN [LEAD].dbo.MaterialUnits mu WITH (NOLOCK)
                       ON  a.AlternateMaterialUnit_id = mu.tid
                  LEFT JOIN [LEAD].dbo.Locations lo WITH (NOLOCK)
                       ON  lo.LocationName = a.SourceLocationName
                  LEFT JOIN [LEAD].dbo.TechnoZones tz
                       ON  tz.tid = lo.TechnoZone_id
                  LEFT JOIN PeopleProductionResources ppl WITH (NOLOCK)
                       ON  ppl.ProductionResource_id = b.tid
                  LEFT JOIN people pl WITH (NOLOCK)
