                    <property
                      dataType="System.String"
                      description="Команда SQL для выполнения."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
Путевой COLLATE Cyrillic_General_CI_AS AS [Путевой]
,aa.Заявка COLLATE Cyrillic_General_CI_AS AS [Заявка]
,aa.[Дата отгрузки]
,aa.Отгрузил COLLATE Cyrillic_General_CI_AS AS [Отгрузил]
,SUM(aa.[Кол-во фабричных мест]) AS [Кол-во фабричных мест]
,SUM(aa.[Кол-во сборных мест]) AS [Кол-во сборных мест]
,aa.Ворота COLLATE Cyrillic_General_CI_AS AS [Ворота]
,aa.[Физ. лицо] COLLATE Cyrillic_General_CI_AS AS [Физ.лицо]
,aa.Партнер COLLATE Cyrillic_General_CI_AS Партнер
,aa.Отдел COLLATE Cyrillic_General_CI_AS Отдел
FROM (SELECT DISTINCT
a.ExternalCode AS [Заявка]
,c.WayListNumber AS [Путевой]
,pr1.NameRU AS [Отгрузил]
,g.NameRU AS [Ворота]
--,svp.EndDate AS [Дата отгрузки]
,aa.RecordDate AS [Дата отгрузки]
,ac.[Кол-во фабричных мест]
,ac.[Кол-во сборных мест]
,ac.Статус
,kc.face AS [Физ. лицо]
,a.DebtorPartnerName AS [Партнер]
,bs.RoleName AS [Отдел]
FROM hdr_DeliveryRequest a
JOIN TransportRequests b ON b.RequestTransaction_id = a.Transaction_id
JOIN VisitorsLog c ON c.tid = b.Visitor_id
JOIN Transactions tr   ON tr.ParentTransaction_id = a.Transaction_id
LEFT JOIN TransactionTypes cc    ON cc.tid = tr.TransactionType_id
cross apply (select top 1  svp.visitor_id,min(svp.recorddate) as [recorddate],svp.ProductionResource_id ,svp.Gate_id
from ShipmentVisitorpdtLog svp 
 where svp.Visitor_id=c.tid
group by svp.visitor_id,svp.ProductionResource_id ,svp.Gate_id
order by 2 desc
) aa

LEFT JOIN ProductionResources pr1    ON pr1.tid = aa.ProductionResource_id
LEFT JOIN hdr_Delivery dr    ON dr.Transaction_id = tr.tid
LEFT JOIN Gates g    ON g.tid = aa.Gate_id
LEFT JOIN PeopleProductionResources ppl WITH (NOLOCK)    ON ppl.ProductionResource_id = pr1.tid
LEFT JOIN people pl WITH (NOLOCK)    ON pl.tid = ppl.People_id
LEFT JOIN Users us WITH (NOLOCK)    ON us.People_id = pl.tid
LEFT JOIN [LEAD].dbo.UserRoles ur WITH (NOLOCK)    ON ur.User_id = us.tid
LEFT JOIN [LEAD].dbo.BusinessRoles bs WITH (NOLOCK)    ON bs.tid = ur.Role_id
LEFT JOIN KisCodePR kc WITH (NOLOCK)    ON kc.TidPR = pr1.tid
LEFT JOIN (SELECT DISTINCT
a.LocalValue
,MAX(bb.RecordDate) AS [Макс.дата]
,SUM(bb.MadeCargoPlacesShipped) AS [Кол-во сборных мест]
,SUM(bb.TotalCargoPlacesShipped - bb.MadeCargoPlacesShipped) AS [Кол-во фабричных мест]
,[Статус] =
CASE
WHEN bb.DeliveryStatus = 'CANCELED' THEN 'CANCELED'
WHEN bb.DeliveryStatus = 'PICKED' THEN 'PICKED'
ELSE 'SHIPPED'
END
FROM temp_ElementIntegration_623 a   
JOIN TRANSIT..hdr_DeliveryResponse bb ON bb.ExternalCode = a.TargetValue
GROUP BY a.LocalValue
,bb.DeliveryStatus) ac
ON ac.LocalValue = tr.ParentTransaction_id
WHERE cc.tid &lt;&gt; 41
AND ac.Статус = 'SHIPPED') aa

GROUP BY aa.Ворота
,aa.Путевой
,aa.[Физ. лицо]
,aa.[Дата отгрузки]
,aa.Заявка
,aa.Партнер
,aa.Отгрузил
,aa.Отдел</property>
