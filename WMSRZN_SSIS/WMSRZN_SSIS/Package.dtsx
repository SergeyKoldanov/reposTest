                      description="Команда SQL для выполнения."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @date1 DATETIME
DECLARE @date2 DATETIME
SET @date1 = CAST(ROUND(CAST(GETDATE() AS FLOAT) ,0 ,1) AS DATETIME)- 3

--SET @date1 = '2015-08-01 00:00:00.000'--мм дд гг

SET @date2 = '2027-10-19 23:59:59.820'--мм дд гг



SELECT [Операция] = 'Горячие пополнения' COLLATE Cyrillic_General_CI_AS
      ,COUNT(a.tid)                   AS [Кол]
      ,TaskDate          AS [Дата]
      ,DATEPART(hour ,taskdate)       AS [Час]
      ,c.nameru  COLLATE Cyrillic_General_CI_AS                     AS [Ресурс]
FROM   hdr_ObjectMovement             AS a WITH (NOLOCK)
       JOIN Transactions              AS b WITH (NOLOCK)
            ON  a.Transaction_id = b.tid
       LEFT JOIN ProductionResources  AS c WITH (NOLOCK)
            ON  a.ProductionResource_id = c.tid
WHERE  a.MovementFlowName = 'Пополнение зоны пикинга'
       AND (b.TransactionStatus=1)
       AND (a.CheckedForDelete=0)
       AND a.IsPlaned = 0
       AND a.TaskDate&gt;= @date1
       AND a.FinishDate&lt;= @date2
GROUP BY
       TaskDate
      ,DATEPART(DAY ,taskdate)
      ,DATEPART(hour ,taskdate)
      ,c.nameru
UNION




 ALL

SELECT [Операция] = 'Вымещение из зоны пикинга' COLLATE Cyrillic_General_CI_AS
      ,COUNT(a.tid)                   AS [Кол]
      ,TaskDate          AS [Дата]
      ,DATEPART(hour ,taskdate)       AS [Час]
      ,c.nameru                COLLATE Cyrillic_General_CI_AS       AS [Ресурс]
FROM   hdr_ObjectMovement             AS a WITH (NOLOCK)
       JOIN Transactions              AS b WITH (NOLOCK)
            ON  a.Transaction_id = b.tid
       LEFT JOIN ProductionResources  AS c WITH (NOLOCK)
            ON  a.ProductionResource_id = c.tid
WHERE  a.MovementFlowName = 'Вымещение из зоны пикинга'
       AND (b.TransactionStatus=1)
       AND (a.CheckedForDelete=0)
       AND a.IsPlaned = 0
       AND a.TaskDate&gt;= @date1
       AND a.FinishDate&lt;= @date2
GROUP BY
      TaskDate
      ,c.nameru
      ,DATEPART(DAY ,taskdate)
      ,DATEPART(hour ,taskdate)
      ,c.nameru
UNION

 ALL

