                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">declare @start datetime ,
        @end datetime

             set @start='2016-05-30 16:01:43.747'
             set @end='2029-05-31 16:01:43.747'

set @start=(CAST(round(CAST(GETDATE() as float),0,1) as datetime) - 3)




select 
[Куб]='Привязка к местам' COLLATE Cyrillic_General_CI_AS,
[ОС]=a.SourceObject_id ,
[Кол-во строк]=count(distinct a.SourceObject_id),
[Вес]=a.weight,
[Объем]=a.volume,
[ГПР]=prg.NameRU COLLATE Cyrillic_General_CI_AS,
[Сотрудник]=SUBSTRING( ck.People,0,100) COLLATE Cyrillic_General_CI_AS,
[Отдел]=bs.RoleName COLLATE Cyrillic_General_CI_AS,
[Исх ТЗ]=tz1.nameru COLLATE Cyrillic_General_CI_AS,
[Исх Ячейка]=lo1.locationname COLLATE Cyrillic_General_CI_AS,

[Цел ТЗ]=tz.nameru COLLATE Cyrillic_General_CI_AS,
[Цел Ячейка]=lo.locationname COLLATE Cyrillic_General_CI_AS,

[Дата - год] =datepart(year,a.Date),
[Дата - месяц] =datepart(MONTH,a.Date),
[Дата - день] =datepart(day,a.Date),
[Дата - час] =datepart(hour,a.Date),
[Дата] = a.[Date]


from AllocateLog a
join ProductionResources b with (nolock) on b.tid=a.productionresource_id
join storageobjects so with (nolock) on so.tid=a.SourceObject_id
left join ProductionResourceGroups  prg with (nolock)on prg.tid=b.ProductionResourceGroup_id
left join locations lo with (nolock) on lo.tid=a.location_id
left join technozones tz with (nolock) on tz.tid=lo.TechnoZone_id

left join locations lo1 with (nolock) on lo1.tid=a.SourceLocation_id
left join technozones tz1 with (nolock) on tz1.tid=lo1.TechnoZone_id

left join PeopleProductionResources ppl with (nolock) on ppl.ProductionResource_id=b.tid
left join people pl with (nolock) on pl.tid=ppl.People_id
left join Users us with (nolock) on us.People_id=pl.tid
left join [LEAD].dbo.UserRoles ur with (nolock) on ur.User_id=us.tid
left join [LEAD].dbo.BusinessRoles bs with (nolock) on bs.tid=ur.Role_id

left join KisCodePR kc with (nolock) on kc.TidPR=b.tid
left join KisCode ck with(nolock) on ck.tid=kc.KisCode_id
where a.Date&gt;=@start and a.Date&lt;=@end
--and a.SourceObject_id=15998111
group by

a.SourceObject_id ,

a.weight,
a.volume,
prg.NameRU,
ck.People,
bs.RoleName,
tz.nameru,
lo.locationname,
datepart(year,a.Date),
datepart(MONTH,a.Date),
datepart(day,a.Date),
datepart(hour,a.Date),
a.Date, tz1.nameru, lo1.locationname</property>
