                      description="Команда SQL для выполнения."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Set Dateformat mdy 
declare @date1 datetime
declare @date2 datetime
SET @date1 = CAST(ROUND(CAST(GETDATE() AS FLOAT), 0, 1) AS DATETIME) - 10
--SET @date1 = '2009-05-01 00:00:00.000'
SET @date2 = '2099-05-31 00:00:00.000'

SELECT DISTINCT
  aa.Операция COLLATE Cyrillic_General_CI_AS [Операция]
 ,aa.Отдел COLLATE Cyrillic_General_CI_AS [Отдел]
 ,aa.[Физ. лизцо] COLLATE Cyrillic_General_CI_AS [Физ. лизцо]
 ,aa.Вес AS [Вес]
 ,aa.Объем AS [Объем]
 ,aa.WaveName COLLATE Cyrillic_General_CI_AS [WaveName]
 ,aa.NameRU COLLATE Cyrillic_General_CI_AS AS [ТЗ]
 ,aa.Строки AS [Строки]
 ,aa.Дата
 ,aa.Час
FROM (SELECT
    [Операция] = 'Набор кладовщиками'
   ,[SKU] = a.Material_id
   ,[Кол-во в единицах] = (
    CASE
      WHEN b1.UnitKoeff &gt; 0 THEN a.BaseQuantity / b1.UnitKoeff
      ELSE 0
    END
    )
   ,[Отдел] = bs.RoleName
   ,kc.face AS [Физ. лизцо]
   ,CAST(a.TaskDate AS DATE) AS [Дата]
   ,DATEPART(HOUR, a.TaskDate) AS [Час]
   ,a.tid AS [Строки]
   ,[Вес] = (
    CASE
      WHEN mu.UnitKoeff &gt; 0 THEN a.BaseQuantity / mu.UnitKoeff
      ELSE 0
    END
    ) * mu.BruttoWeight
   ,[Объем] = (
    CASE
      WHEN mu.UnitKoeff &gt; 0 THEN a.BaseQuantity / mu.UnitKoeff
      ELSE 0
    END
    ) * mu.UnitVolume
   ,z.WaveName
   ,tz.NameRU
  FROM [LEAD].dbo.hdr_MaterialPicking AS a WITH (NOLOCK)
  JOIN [LEAD].dbo.ProductionResources b WITH (NOLOCK)
    ON b.tid = a.ProductionResource_id
  left JOIN [LEAD].dbo.tbl_WaveTare wt WITH (NOLOCK)
    ON wt.tid = a.WaveTare_id
  LEFT JOIN [LEAD].dbo.Waves AS j WITH (NOLOCK)
    ON a.Wave_id = j.tid
  LEFT JOIN [LEAD].dbo.WaveTypes AS z WITH (NOLOCK)
    ON j.WaveType_id = z.tid
  LEFT JOIN [LEAD].dbo.Materials ma WITH (NOLOCK)
    ON ma.tid = a.Material_id
  LEFT JOIN [LEAD].dbo.MaterialUnits mu WITH (NOLOCK)
    ON a.AlternateMaterialUnit_id = mu.tid
  LEFT JOIN [LEAD].dbo.Locations lo WITH (NOLOCK)
    ON lo.LocationName = a.SourceLocationName
  LEFT JOIN [LEAD].dbo.TechnoZones tz WITH (NOLOCK)
    ON tz.tid = lo.TechnoZone_id
  LEFT JOIN PeopleProductionResources ppl WITH (NOLOCK)
    ON ppl.ProductionResource_id = b.tid
  LEFT JOIN people pl WITH (NOLOCK)
    ON pl.tid = ppl.People_id
  LEFT JOIN Users us WITH (NOLOCK)
    ON us.People_id = pl.tid
  LEFT JOIN [LEAD].dbo.UserRoles ur WITH (NOLOCK)
    ON ur.User_id = us.tid
  LEFT JOIN [LEAD].dbo.BusinessRoles bs WITH (NOLOCK)
    ON bs.tid = ur.Role_id
  LEFT JOIN MaterialUnits AS b1 WITH (NOLOCK)
    ON a.AlternateMaterialUnit_id = b1.tid
  LEFT JOIN KisCodePR kc WITH (NOLOCK)
    ON kc.TidPR = b.tid
  WHERE z.WaveName NOT LIKE '%Поп%'
  AND z.WaveName NOT LIKE '%Комис%'
  AND z.WaveName NOT LIKE '%ЗПХ%'
  AND z.WaveName NOT LIKE '%Мебе%'
  AND a.FinishDate &lt;= @date2
  AND a.TaskDate &gt;= @date1

  UNION ALL

  SELECT DISTINCT
    [Операция] = 'Холодные пополнения кладовщиками '
   ,[SKU] = a.Material_id
   ,[Кол-во в единицах] = (
    CASE
      WHEN b1.UnitKoeff &gt; 0 THEN a.BaseQuantity / b1.UnitKoeff
      ELSE 0
    END
    )
   ,[Отдел] = bs.RoleName
   ,
    --b.NameRU as [Пользователь],
    kc.face AS [Физ. лизцо]
   ,CAST(a.TaskDate AS DATE) AS [Дата]
   ,DATEPART(HOUR, a.TaskDate) AS [Час]
   ,a.tid AS [Строки]
   ,[Вес] = (
    CASE
      WHEN mu.UnitKoeff &gt; 0 THEN a.BaseQuantity / mu.UnitKoeff
      ELSE 0
    END
    ) * mu.BruttoWeight
   ,[Объем] = (
    CASE
      WHEN mu.UnitKoeff &gt; 0 THEN a.BaseQuantity / mu.UnitKoeff
      ELSE 0
    END
    ) * mu.UnitVolume
   ,z.WaveName
   ,tz.NameRU
  FROM [LEAD].dbo.hdr_MaterialPicking AS a WITH (NOLOCK)
  JOIN [LEAD].dbo.ProductionResources b WITH (NOLOCK)
    ON b.tid = a.ProductionResource_id
  left JOIN [LEAD].dbo.tbl_WaveTare wt WITH (NOLOCK)
    ON wt.tid = a.WaveTare_id
  LEFT JOIN [LEAD].dbo.Waves AS j WITH (NOLOCK)
    ON a.Wave_id = j.tid
