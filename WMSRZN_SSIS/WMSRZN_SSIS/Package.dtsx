                    <property
                      dataType="System.String"
                      description="Команда SQL для выполнения."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SET DATEFORMAT mdy

DECLARE @date1 DATETIME
DECLARE @date2 DATETIME

SET @date1 = '2008-12-01 00:00:00.000'
SET @date2 = '2027-08-15 23:59:59.297'


SELECT DISTINCT aa.NameRU COLLATE Cyrillic_General_CI_AS AS [ТЗ]
      ,aa.WaveName COLLATE Cyrillic_General_CI_AS AS [Волна]
      ,aa.Год
      ,aa.Месяц
      ,aa.День
      ,aa.Час
      ,aa.Отдел COLLATE Cyrillic_General_CI_AS Отдел
      ,aa.[Кол-во в единицах]
      ,aa.[Строки]
      ,aa.[Вес]
      ,aa.[Объем]
      ,aa.[SKU]  AS [Вид Материала]
      ,aa.[Длительность,c]
      ,aa.[Физ. лизцо] COLLATE Cyrillic_General_CI_AS [Физ. лизцо]
      ,aa.Операция COLLATE Cyrillic_General_CI_AS [Операция]
      ,aa.TaskDate [data]
	  ,cast(isnull(aa.[ШК Ролл],'--') as nvarchar) as [ШК Ролл]
FROM   (
           SELECT DISTINCT [Длительность,c] = DATEDIFF(SECOND ,a.TaskDate ,a.FinishDate)
                 ,[Операция] = (case when Z.WaveName NOT LIKE '%Поп%' AND Z.WaveName NOT LIKE '%Комис%'  AND Z.WaveName NOT LIKE '%ЗПХ%' then 'Набор кладовщиками'
									 when Z.WaveName LIKE '%Хол%' AND Z.WaveName NOT LIKE '%ЗПХ%' AND Z.WaveName NOT LIKE '%Комис%' then 'Холодные пополнения кладовщиками'
									 when Z.WaveName NOT LIKE '%Хол%' AND Z.WaveName LIKE '%Попо%' AND Z.WaveName NOT LIKE '%ЗПХ%' AND Z.WaveName NOT LIKE '%КОМИ%' then 'Горячие пополнения кладовщиками'
									 when Z.WaveName NOT LIKE '%Хол%' AND Z.WaveName LIKE '%Попо%' AND Z.WaveName NOT LIKE '%С01 и С02%' AND Z.WaveName NOT LIKE '%КОМИС%' AND Z.WaveName LIKE '%ЗПХ%' then 'Г\П ПРТ'
									 when Z.WaveName LIKE '%ХОЛ%' AND Z.WaveName LIKE '%ЗПХ%' then 'Х\П ПРТ'
									 when Z.WaveName LIKE '%КОМ%' AND Z.WaveName LIKE '%ПОП%' then 'Х\П, Г\П Комиссионер'
									 when (Z.WaveName LIKE '%ЗПХ%' OR Z.WaveName LIKE '%Комис%' OR Z.WaveName LIKE '%ЗПХ Мебе%') AND Z.WaveName NOT LIKE '%ПОП%' then 'Набор ПРТ\Комис'
								end)
                 ,[SKU]                   = a.Material_id
                 ,[Кол-во в единицах]     = (CASE WHEN b1.UnitKoeff&gt;0 THEN a.BaseQuantity/b1.UnitKoeff
											 ELSE 0
											 END)
                 ,[Отдел]                 = bs.RoleName
                 ,kc.Face  AS [Физ. лизцо]
                 ,DATEPART(HOUR ,a.TaskDate) AS [Час]
                 ,DATEPART(YEAR ,a.TaskDate) AS [Год]
                 ,DATEPART(DAY ,a.TaskDate) AS [День]
                 ,DATEPART(MONTH ,a.TaskDate) AS [Месяц]
                 ,a.tid    AS [Строки]
                 ,[Вес] = (CASE WHEN mu.UnitKoeff&gt;0 THEN a.BaseQuantity/mu.UnitKoeff
                           ELSE 0
                           END)*mu.BruttoWeight
                 ,[Объем] = (CASE WHEN mu.UnitKoeff&gt;0 THEN a.BaseQuantity/mu.UnitKoeff
							 ELSE 0
							 END)*mu.UnitVolume
                 ,Z.WaveName
                 ,tz.NameRU
                 ,CAST(a.TaskDate AS DATE) TaskDate
				 ,rc.Barcode as [ШК Ролл]
           FROM   hdr_MaterialPicking AS a WITH (NOLOCK)
                  JOIN ProductionResources b WITH (NOLOCK) ON  b.tid = a.ProductionResource_id
                  JOIN tbl_WaveTare wt WITH (NOLOCK) ON  wt.tid = a.WaveTare_id
                  LEFT JOIN Waves AS j WITH (NOLOCK) ON  a.Wave_id = j.tid
                  LEFT JOIN WaveTypes AS Z WITH (NOLOCK) ON  j.WaveType_id = Z.tid
                  LEFT JOIN Materials MA WITH (NOLOCK) ON  MA.tid = a.Material_id
                  LEFT JOIN MaterialUnits mu WITH (NOLOCK) ON  a.AlternateMaterialUnit_id = mu.tid
                  LEFT JOIN Locations lo WITH (NOLOCK) ON  lo.tid = a.SourceLocation_id
                  LEFT JOIN TechnoZones tz WITH (NOLOCK) ON  tz.tid = lo.TechnoZone_id
                  LEFT JOIN PeopleProductionResources ppl WITH (NOLOCK) ON  ppl.ProductionResource_id = b.tid
                  LEFT JOIN People pl WITH (NOLOCK) ON  pl.tid = ppl.People_id
                  LEFT JOIN Users us WITH (NOLOCK) ON  us.People_id = pl.tid
                  LEFT JOIN UserRoles ur WITH (NOLOCK) ON  ur.User_id = us.tid
                  LEFT JOIN BusinessRoles bs WITH (NOLOCK) ON  bs.tid = ur.Role_id
                  LEFT JOIN MaterialUnits AS b1 WITH (NOLOCK) ON  a.AlternateMaterialUnit_id = b1.tid
                  LEFT JOIN KisCodePR kc WITH (NOLOCK) ON  kc.TidPR = b.tid
				  LEFT JOIN RollContainer rc WITH (NOLOCK) ON  rc.tid = wt.RollContainer_id
           WHERE  a.TaskDate&lt;= @date2
                  AND a.TaskDate&gt;= @date1
       )            aa</property>
