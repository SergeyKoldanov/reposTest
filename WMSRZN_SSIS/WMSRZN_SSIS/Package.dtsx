                    <property
                      dataType="System.String"
                      description="Команда SQL для выполнения."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @start DATETIME
       ,@end DATETIME

SET @end = '20271215'

SET @start = (CAST(ROUND(CAST(GETDATE() AS FLOAT), 0, 1) AS DATETIME) - 3)

--SET @start = '2015-12-14 16:01:43.747'


SELECT
  [Куб] = 'Раскладка по заданию' COLLATE Cyrillic_General_CI_AS
 ,[Товаропоток] = so.NameRU COLLATE Cyrillic_General_CI_AS
 ,[Дата записи - год] = DATEPART(YEAR, a.recorddate)
 ,[Дата записи - месяц] = DATEPART(MONTH, a.recorddate)
 ,[Дата записи - день] = DATEPART(DAY, a.recorddate)
 ,[Дата записи - час] = DATEPART(HOUR, a.recorddate)
 ,[Кол-во строк] = COUNT(DISTINCT a.tid)
 ,[ОС] = a.StorageObject_id

 ,[В ТЗ] = tz.NameRU COLLATE Cyrillic_General_CI_AS
 ,[В яч] = lo.LocationName COLLATE Cyrillic_General_CI_AS
 ,[Объем] =
  --case when a.volume =0 then (aa.basequantity/mu.unitkoeff)*mu.unitvolume else  a.volume end,
  CASE
    WHEN mu.unitkoeff = 0 THEN 0
    ELSE CASE
        WHEN a.volume = 0 THEN (aa.basequantity / mu.unitkoeff) * mu.unitvolume
        ELSE a.volume
      END
  END
 ,[Вес] =
  CASE
    WHEN a.weight = 0 THEN (aa.basequantity / mu.UnitKoeff) * mu.BruttoWeight
    ELSE a.weight
  END
 ,---((ws.BaseQuantity/j.UnitKoeff)*j.BruttoWeight)
  [Кол-во] =
  CASE
    WHEN a.quantity = 0 THEN FLOOR(aa.BaseQuantity) /
      CASE
        WHEN KOeff = 0 THEN 1
        ELSE koeff
      END
    ELSE FLOOR(a.quantity /
      CASE
        WHEN KOeff = 0 THEN 1
        ELSE koeff
      END)
  END
 ,[Кол-во в базовых ед] =
  CASE
    WHEN a.quantity = 0 THEN FLOOR(aa.BaseQuantity)
    ELSE FLOOR(a.quantity)
  END
 ,[Коэфф] = a.koeff
 ,[Товар] = ma.TasteEN COLLATE Cyrillic_General_CI_AS
 ,[Наименоваение] = ma.nameru COLLATE Cyrillic_General_CI_AS
 ,[Ресурс] = kc.face COLLATE Cyrillic_General_CI_AS
 ,[Отдел] = bs.RoleName COLLATE Cyrillic_General_CI_AS
 ,[Дата] = a.recorddate
FROM Log_Putaway_Main a
LEFT JOIN (SELECT DISTINCT
    l.tid
   ,cl.Material_id
   ,cl.BaseQuantity
  FROM complectationlog cl
  LEFT JOIN ProductionResources PR WITH (NOLOCK)
    ON PR.TID = CL.ProductionResource_id
  LEFT JOIN StorageObjects so WITH (NOLOCK)
    ON so.tid = CL.TargetStorageObject_id
  LEFT JOIN Warehouses w WITH (NOLOCK)
    ON w.tid = so.Warehouse_id
  LEFT JOIN Locations L WITH (NOLOCK)
    ON L.tid = SO.Location_id
  LEFT JOIN Materials M WITH (NOLOCK)
    ON M.tid = CL.Material_id
  LEFT JOIN barcodeObjects b WITH (NOLOCK)
    ON b.tid = CL.targetBarcodeObject_id
  LEFT JOIN Debtors d WITH (NOLOCK)
    ON d.tid = b.OwnerDebtor_id
  LEFT JOIN StockTypes st WITH (NOLOCK)
    ON st.tid = CL.TargetStockType_id
  LEFT JOIN StockCategories sc WITH (NOLOCK)
    ON sc.tid = CL.targetStockCategory_id) aa
  ON aa.tid = a.TargetLocation_id
    AND aa.Material_id = a.material_id
LEFT JOIN locations lo WITH (NOLOCK)
  ON lo.tid = a.TargetLocation_id
LEFT JOIN technozones tz WITH (NOLOCK)
  ON tz.tid = lo.TechnoZone_id
LEFT JOIN materials ma WITH (NOLOCK)
  ON ma.tid = a.material_id
LEFT JOIN ProductionResources pr WITH (NOLOCK)
  ON pr.tid = a.Productionresource_Id
LEFT JOIN MaterialUnits mu WITH (NOLOCK)
  ON mu.tid = a.Unit_ID
LEFT JOIN PeopleProductionResources ppl WITH (NOLOCK)
  ON ppl.ProductionResource_id = pr.tid
LEFT JOIN people pl WITH (NOLOCK)
  ON pl.tid = ppl.People_id
LEFT JOIN Users us WITH (NOLOCK)
  ON us.People_id = pl.tid
LEFT JOIN [LEAD].dbo.UserRoles ur WITH (NOLOCK)
  ON ur.User_id = us.tid
LEFT JOIN [LEAD].dbo.BusinessRoles bs WITH (NOLOCK)
  ON bs.tid = ur.Role_id
LEFT JOIN KisCodePR kc WITH (NOLOCK)
  ON kc.TidPR = pr.tid
LEFT JOIN MovementFlows so
  ON so.tid = a.movementflow_id

WHERE a.recorddate &gt;= @start

AND a.recorddate &lt;= @end
GROUP BY DATEPART(YEAR, a.recorddate)
        ,DATEPART(MONTH, a.recorddate)
        ,DATEPART(DAY, a.recorddate)
        ,DATEPART(HOUR, a.recorddate)
        ,a.StorageObject_id
        ,tz.NameRU
        ,lo.LocationName
        ,
         --case when a.volume =0 then (aa.basequantity/mu.unitkoeff)*mu.unitvolume else  a.volume end,
         CASE
           WHEN mu.unitkoeff = 0 THEN 0
           ELSE CASE
               WHEN a.volume = 0 THEN (aa.basequantity / mu.unitkoeff) * mu.unitvolume
               ELSE a.volume
             END
         END
        ,CASE
           WHEN a.weight = 0 THEN (aa.basequantity / mu.UnitKoeff) * mu.BruttoWeight
           ELSE a.weight
         END
        ,CASE
           WHEN a.quantity = 0 THEN FLOOR(aa.BaseQuantity) /
             CASE
               WHEN KOeff = 0 THEN 1
               ELSE koeff
             END
           ELSE FLOOR(a.quantity /
             CASE
               WHEN KOeff = 0 THEN 1
               ELSE koeff
             END)
         END
        ,CASE
           WHEN a.quantity = 0 THEN FLOOR(aa.BaseQuantity)
           ELSE FLOOR(a.quantity)
         END
        ,a.koeff
        ,ma.TasteEN
        ,ma.nameru
        ,kc.face
        ,bs.RoleName
        ,so.NameRU
        ,a.recorddate
        ,a.MasterObject_id</property>
