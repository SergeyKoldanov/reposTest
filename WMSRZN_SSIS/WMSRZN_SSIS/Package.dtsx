                      description="Команда SQL для выполнения."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @BeginDate DATETIME
DECLARE @EndDate DATETIME

SET @BeginDate = '20151010'
SET @EndDate = '20271115'

SELECT DATEPART(YEAR, ISNULL(hdr.EndControlDate, hdr.EndRevisionDate)) AS Год,
       DATEPART(MONTH, ISNULL(hdr.EndControlDate, hdr.EndRevisionDate)) AS Месяц,
       DATEPART(DAY, ISNULL(hdr.EndControlDate, hdr.EndRevisionDate)) AS День,
       DATEPART(HOUR, ISNULL(hdr.EndControlDate, hdr.EndRevisionDate)) AS Час,
       COUNT(DISTINCT hdr.SSCC)   AS КолвоКоробов,
       COUNT(tbl.MaterialCode)    AS КолвоСтрок,
       hdr.ControllerLocalCode  COLLATE Cyrillic_General_CI_AS  AS [Код контроллера из 1с],
       hdr.ControllerName COLLATE Cyrillic_General_CI_AS        AS [ФИО]
FROM   rc_hdr_DoubleControl   (NOLOCK)    AS hdr
       JOIN rc_tbl_DoubleControl  (NOLOCK) AS tbl
            ON  hdr.sscc = tbl.sscc
WHERE  hdr.STATUS &gt; 10
       AND ISNULL(hdr.EndControlDate, hdr.EndRevisionDate) &gt;= @BeginDate
       AND ISNULL(hdr.EndControlDate, hdr.EndRevisionDate) &lt; @EndDate
GROUP BY
       DATEPART(YEAR, ISNULL(hdr.EndControlDate, hdr.EndRevisionDate)),
       DATEPART(MONTH, ISNULL(hdr.EndControlDate, hdr.EndRevisionDate)),
       DATEPART(DAY, ISNULL(hdr.EndControlDate, hdr.EndRevisionDate)),
       DATEPART(HOUR, ISNULL(hdr.EndControlDate, hdr.EndRevisionDate)),
       hdr.ControllerLocalCode,
       hdr.ControllerName
ORDER BY
       Год,
       Месяц,
       День,
       Час</property>
