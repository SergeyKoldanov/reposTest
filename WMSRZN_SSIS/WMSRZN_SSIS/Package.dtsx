                    <property
                      dataType="System.String"
                      description="Команда SQL для выполнения."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @start     DATETIME
       ,@end       DATETIME

SET @start = '2015-12-01 16:01:43.747'
SET @end = '2027-12-31 16:01:43.747'
--SET @start =  (CAST(round(CAST(GETDATE() as float),0,1) as datetime) - 3)

SELECT [Куб] = 'Ручное перемещение' COLLATE Cyrillic_General_CI_AS
      ,[Ресурс]           = SUBSTRING ( kc1.People ,1 , 100) COLLATE Cyrillic_General_CI_AS
      ,[Отдел]            = bs.RoleName COLLATE Cyrillic_General_CI_AS
      ,[Код товара]       = mat.TasteEN COLLATE Cyrillic_General_CI_AS
      ,[Наименование]     = mat.NameRU COLLATE Cyrillic_General_CI_AS
      ,[Коэф]             = ma1.UnitKoeff
      ,SUM(FLOOR(a.MoveQuantity/ma1.UnitKoeff)) AS [Кол-во]
      ,COUNT(DISTINCT a.SourceObject_id)  AS [Кол-во ОС]
      ,SUM(a.weight)                      AS [Вес]
      ,SUM(a.volume)                      AS [Объем]
      ,[Упак] = u1.NameRU COLLATE            Cyrillic_General_CI_AS
      ,[ИЗ яч] = ISNULL(lo1.LocationName ,a.SourceObject_id) COLLATE Cyrillic_General_CI_AS
      ,[Из ТЗ] = tz1.nameru COLLATE          Cyrillic_General_CI_AS
      ,a.SourceObjectBarcode
      ,[Из ОС] = a.SourceObject_id
      ,[В ТЗ] = tz.NameRU COLLATE            Cyrillic_General_CI_AS
      ,[В яч] = ISNULL(lo.LocationName ,a.TargetObject_ID) COLLATE Cyrillic_General_CI_AS
      ,ObjectLocation = dbo.ObjectLocation(a.TargetObject_ID) COLLATE Cyrillic_General_CI_AS
      ,a.TargetObject_ID                  AS [В ОС]
      ,DATEPART(YEAR ,a.Date)             AS [Год]
      ,DATEPART(MONTH ,a.Date)            AS [Месяц]
      ,DATEPART(DAY ,a.Date)              AS [День]
      ,DATEPART(hour ,a.Date)             AS [Час]
--,a.date [Дата] 
FROM   [lead].dbo.handmade a
       JOIN [lead].dbo.materialunits ma1 WITH (NOLOCK)
            ON  ma1.tid = a.alternatematerialunit_id
       JOIN [lead].dbo.units u1 WITH(NOLOCK)
            ON  u1.tid = ma1.Unit_id
       JOIN [lead].dbo.materials mat WITH(NOLOCK)
            ON  mat.tid = a.Material_id
       LEFT JOIN [lead].dbo.storageobjects so1 WITH(NOLOCK)
            ON  so1.tid = a.SourceObject_id
       LEFT JOIN [lead].dbo.locations lo1 WITH(NOLOCK)
            ON  lo1.tid = so1.Location_id
       LEFT JOIN [lead].dbo.technozones tz1 WITH(NOLOCK)
            ON  tz1.tid = lo1.TechnoZone_id
       LEFT JOIN [lead].dbo.ProductionResources pr WITH(NOLOCK)
            ON  pr.tid = a.productionresource_id
       LEFT JOIN [lead].dbo.storageobjects so WITH(NOLOCK)
            ON  so.tid = a.TargetObject_ID
       LEFT JOIN [lead].dbo.loCATIONS LO WITH(NOLOCK)
            ON  lo.tid = so.Location_id
       LEFT JOIN [lead].dbo.technozones tz WITH(NOLOCK)
            ON  tz.tid = lo.TechnoZone_id
       LEFT JOIN [lead].dbo.PeopleProductionResources ppl WITH(NOLOCK)
            ON  ppl.ProductionResource_id = pr.tid
       LEFT JOIN [lead].dbo.people pl WITH(NOLOCK)
            ON  pl.tid = ppl.People_id
       LEFT JOIN [lead].dbo.Users us WITH(NOLOCK)
            ON  us.People_id = pl.tid
       LEFT JOIN [LEAD].dbo.UserRoles ur WITH(NOLOCK)
            ON  ur.User_id = us.tid
       LEFT JOIN [LEAD].dbo.BusinessRoles bs WITH(NOLOCK)
            ON  bs.tid = ur.Role_id
       LEFT JOIN [lead].dbo.KisCodePR kc WITH(NOLOCK)
            ON  kc.TidPR = pr.tid
       LEFT JOIN kiscode kc1 WITH(NOLOCK)
            ON  kc1.tid = kc.KisCode_id
WHERE  a.Date&gt;= @start
       AND a.date&lt;= @end
       AND ma1.UnitKoeff&gt;0
GROUP BY
       kc1.People
      ,mat.TasteEN
      ,ma1.UnitKoeff
      ,bs.RoleName
      ,u1.NameRU
      ,ISNULL(lo1.LocationName ,a.SourceObject_id)
      ,a.SourceObjectBarcode
      ,a.SourceObject_id
      ,ISNULL(lo.LocationName ,a.TargetObject_ID)
      ,a.TargetObject_ID
      ,DATEPART(YEAR ,a.Date)
      ,DATEPART(MONTH ,a.Date)
      ,DATEPART(DAY ,a.Date)
      ,DATEPART(hour ,a.Date)
      ,mat.NameRU
      ,tz1.nameru
      ,tz.NameRU
--,a.date</property>
