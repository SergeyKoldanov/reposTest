                    <property
                      dataType="System.String"
                      description="Команда SQL для выполнения."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">--SET DATEFORMAT mdy
DECLARE @date1 DATETIME
DECLARE @date2 DATETIME
DECLARE @s CHAR(8)
SET @date1 = CAST(ROUND(CAST(GETDATE() AS FLOAT), 0, 1) AS DATETIME) - 3
--SET @date1 = '20181201'

SET @date2 = '20250101'

SELECT DISTINCT 
	[Кол-во строк] = aa.[Строки],
    [Заказ] = aa.PurchaseNumber COLLATE Cyrillic_General_CI_AS,
    [Ресурс] = aa.[Физ. лизцо] COLLATE Cyrillic_General_CI_AS,
    aa.[Объем],
	aa.[Вес],
    [ТЗ] = aa.NameRU COLLATE Cyrillic_General_CI_AS,
    [Время начала] = aa.TaskDate,
    [Время окончания] = aa.FinishDate,
    aa.[Прод-ть в М],
    aa.[Прод-ть в С],
    [Отдел] = aa.Отдел COLLATE Cyrillic_General_CI_AS,
    [Операция] = aa.Операция COLLATE Cyrillic_General_CI_AS,
    [Имя волны] = aa.WaveName COLLATE Cyrillic_General_CI_AS,
    aa.[Кол-во в единицах],
    [Единица] = case when aa.Единица = 3 then 'Шт' else 'Упак' end COLLATE Cyrillic_General_CI_AS
FROM (
    SELECT DISTINCT 
		[Прод-ть в С] = DATEDIFF(second, a.taskdate, a.finishdate),
        [Прод-ть в М] = DATEDIFF(MINUTE, a.taskdate, a.finishdate),
        [Операция] = CASE WHEN 
						z.WaveName NOT LIKE '%Поп%'
						AND z.WaveName NOT LIKE '%Комис%'
						AND z.WaveName NOT LIKE '%ЗПХ%'
					 THEN 'Набор кладовщиками'
					 ELSE CASE WHEN 
						z.WaveName LIKE '%Хол%'
						AND z.WaveName NOT LIKE '%ЗПХ%'
						AND z.WaveName NOT LIKE '%Комис%'
					 THEN 'Холодные пополнения кладовщиками'
					 ELSE CASE WHEN
						WaveName NOT LIKE '%Хол%'
						AND WaveName LIKE '%Попо%'
						AND WaveName NOT LIKE '%ЗПХ%'
						AND WaveName NOT LIKE '%КОМИ%'
					 THEN 'Горячие пополнения кладовщиками'
					 ELSE CASE WHEN
						z.WaveName NOT LIKE '%Хол%'
						AND z.WaveName LIKE '%Попо%'
						AND z.WaveName NOT LIKE '%С01 и С02%'
						AND z.WaveName NOT LIKE '%КОМИС%'
						AND z.WaveName LIKE '%ЗПХ%'
					 THEN 'Г\П ПРТ'
					 ELSE CASE WHEN
						z.WaveName LIKE '%ХОЛ%'
						AND z.WaveName LIKE '%ЗПХ%'
					 THEN 'Х\П ПРТ'
					 ELSE CASE WHEN
						z.WaveName LIKE '%КОМ%'
						AND z.WaveName LIKE '%ПОП%'
					 THEN 'Х\П, Г\П Комиссионер'
					 ELSE CASE WHEN
						(z.wavename LIKE '%ЗПХ%' OR z.wavename LIKE '%Комис%')
						AND z.wavename NOT LIKE '%ПОП%'
					 THEN 'Набор ПРТ\Комис' END END END END END END END,
        [SKU] = a.Material_id,
        [Кол-во в единицах] = CASE WHEN b1.UnitKoeff&gt;0 THEN a.BaseQuantity/b1.UnitKoeff ELSE 0 END,
        [Отдел] = bs.RoleName,
        [Физ. лизцо] = kc.face,
        [Час] = DATEPART(HOUR, a.TaskDate),
        [Год] = DATEPART(YEAR, a.TaskDate),
        [День] = DATEPART(DAY, a.TaskDate),
        [Месяц] = DATEPART(MONTH, a.TaskDate),
        a.TaskDate,
        a.FinishDate,
        [Строки] = a.tid,
        [Вес] = (CASE WHEN mu.UnitKoeff&gt;0 THEN a.BaseQuantity/mu.UnitKoeff ELSE 0 END) * mu.BruttoWeight,
        [Объем] = (CASE WHEN mu.UnitKoeff&gt;0 THEN a.BaseQuantity/mu.UnitKoeff ELSE 0 END)*mu.UnitVolume,
        z.WaveName,
        tz.NameRU,
        k.PurchaseNumber,
        [Единица] = a.PackType_id
    FROM dbo.hdr_MaterialPicking a WITH (NOLOCK)
        JOIN dbo.ProductionResources b WITH (NOLOCK)
            ON b.tid = a.ProductionResource_id
        JOIN dbo.tbl_WaveTare wt WITH (NOLOCK)
            ON wt.tid = a.WaveTare_id
        LEFT JOIN dbo.Waves j WITH (NOLOCK)
            ON a.Wave_id = j.tid
        LEFT JOIN dbo.WaveTypes z WITH (NOLOCK)
            ON j.WaveType_id = z.tid
        LEFT JOIN dbo.Materials ma WITH (NOLOCK)
            ON ma.tid = a.Material_id
        LEFT JOIN dbo.MaterialUnits mu WITH (NOLOCK)
            ON a.AlternateMaterialUnit_id = mu.tid
        LEFT JOIN dbo.Locations lo WITH (NOLOCK)
            ON lo.tid = a.SourceLocation_id
        LEFT JOIN dbo.TechnoZones tz WITH (NOLOCK)
            ON tz.tid = lo.TechnoZone_id
        LEFT JOIN PeopleProductionResources ppl WITH (NOLOCK)
            ON ppl.ProductionResource_id = b.tid
        LEFT JOIN people pl WITH (NOLOCK)
            ON pl.tid = ppl.People_id
        LEFT JOIN Users us WITH (NOLOCK)
            ON us.People_id = pl.tid
        LEFT JOIN dbo.UserRoles ur WITH (NOLOCK)
            ON ur.User_id = us.tid
        LEFT JOIN dbo.BusinessRoles bs WITH (NOLOCK)
            ON bs.tid = ur.Role_id
        LEFT JOIN MaterialUnits b1 WITH (NOLOCK)
            ON a.AlternateMaterialUnit_id = b1.tid
        LEFT JOIN KisCodePR kc WITH (NOLOCK)
            ON kc.TidPR = b.tid
        LEFT JOIN dbo.Transactions e WITH (NOLOCK)
            ON a.Transaction_id = e.tid
        LEFT JOIN dbo.hdr_Delivery k WITH (NOLOCK)
            ON e.ParentTransaction_id = k.Transaction_id
    WHERE a.TaskDate BETWEEN @date1 AND @date2
) aa</property>
