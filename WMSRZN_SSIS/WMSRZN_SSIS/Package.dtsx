                      description="Команда SQL для выполнения."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @Start DATETIME
DECLARE @end DATETIME
SET @end = '20281230'

SET @Start = (CAST(round(CAST(GETDATE() as float),0,1) as datetime) - 3)



SELECT
0   AS [SKU]
,'' COLLATE Cyrillic_General_CI_AS AS [ОС]
,'' COLLATE Cyrillic_General_CI_AS AS [Заявка]
,''COLLATE Cyrillic_General_CI_AS AS [Поставка]
,a.RecordDate AS [Дата]
,FLOOR(SUM(a.Quantity * c.NettoWeight)) AS [Вес]
,SUM(a.Quantity * c.UnitVolume) AS [Объем]
, pr.NameRU COLLATE Cyrillic_General_CI_AS AS [Сотрудник]
,COUNT(a.Material_id) AS [Кол-во операций]
,d.TasteEN COLLATE Cyrillic_General_CI_AS AS  Материал
,d.NameRU COLLATE Cyrillic_General_CI_AS AS  Наименование

FROM hdr_WarehouseIncome wi WITH (NOLOCK)
JOIN Transactions tr WITH (NOLOCK) ON tr.tid = wi.Transaction_id
LEFT JOIN VisitorsLog vl WITH (NOLOCK) ON wi.Transport_id = vl.tid
JOIN tbl_WarehouseIncomeMaterials a WITH (NOLOCK) ON a.Transaction_id = wi.Transaction_id
JOIN MaterialUnits AS c WITH (NOLOCK)ON a.MaterialUnit_id = c.tid
JOIN Materials AS d WITH (NOLOCK) ON a.Material_id = d.tid
LEFT JOIN ProductionResources pr WITH (NOLOCK) ON pr.tid = a.ProductionResource_id
WHERE a.RecordDate &lt;= @end
AND a.RecordDate &gt;= @Start

GROUP BY pr.NameRU
        ,a.RecordDate
        ,wi.tid
             ,d.TasteEN 
             ,d.NameRU</property>
