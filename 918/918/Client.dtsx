                <property
                  dataType="System.String"
                  description="Указывает переменную, содержащую имя объекта базы данных, используемого для открытия набора строк."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="Команда SQL для выполнения."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
	Kontr._Code AS [Код клиента], 
	Kontr._Description AS [Наименование клиента],
	KontrG._Code AS [Код головного клиента],
	KontrG._Description AS [Наименование головного клиента],
	Kontr._IDRRef AS [Ссылка на клиента],
	Kontr._ParentIDRRef AS [Ссылка на родителя],
	SegmV._Description AS [Характеристика],
	Region._Description AS [Регион],
	Country._Description AS [Страна],
	Users._Description AS [Основной менеджер покупателя],
	CASE
		WHEN Kontr._Folder = 0x00 THEN 1
		WHEN Kontr._Folder = 0x01 THEN 0
	END AS [Это группа],
	CASE
		WHEN Kontr._Marked = 0x00 THEN 0
		WHEN Kontr._Marked = 0x01 THEN 1
	END AS [Пометка на удаление]
FROM [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference47] AS Kontr WITH (NOLOCK)
	LEFT JOIN [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference47] AS KontrG WITH (NOLOCK)
		ON KontrG._IDRRef = Kontr._Fld638RRef
	LEFT JOIN (SELECT *
			   FROM [SQL-NODE-01].[RelefUTWork].[dbo].[_InfoRg25409] WITH (NOLOCK)
			   WHERE _Fld25413RRef = 0x80C230E1715C531711E91E7C60562488) AS RegSegm
		ON Kontr._IDRRef = RegSegm._Fld25410RRef
	LEFT JOIN [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference25400] AS SegmV WITH (NOLOCK)
		ON RegSegm._Fld25414_RRRef = SegmV._IDRRef
	LEFT JOIN [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference66] AS Region WITH (NOLOCK)
		ON Kontr._Fld660RRef = Region._IDRRef
	LEFT JOIN [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference60] AS Users WITH (NOLOCK)
		ON Kontr._Fld651RRef = Users._IDRRef
	LEFT JOIN [SQL-NODE-01].[RelefUTWork].[dbo].[_Reference44] AS Country WITH (NOLOCK)
		ON Kontr._Fld19058RRef = Country._IDRRef</property>
                <property
