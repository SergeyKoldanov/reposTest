	  ,SUM(O.Amount * O.Price) AS Total
	  ,I.Code AS [Код товара]
	  ,I.[Name] AS [НаименованиеТовара]
	  ,I.Article AS [АртикулТовара]
	  ,I.Class AS [КлассТовара]
	  ,I.[Group] AS [ГруппаТовара]
	  ,I.Categoty AS [КатегорияТовара]
	  ,I.SubCategory AS [ПодкатегорияТовара]
	  ,I.TradeMark AS [ТорговаяМарка]
	  ,I.Manufacturer AS [ПроизводительТовара]
	  
FROM EndClients918 AS C
	LEFT JOIN Orders918 AS O
		ON C.Id = O.ClientId
	LEFT JOIN [dbo].[Items] I
		ON O.ItemGuid = cast([dbo].[BinaryToGUID](Ref) as char(36))
	LEFT JOIN Shops918 AS S
		ON O.ShopId = S.Id
	LEFT JOIN Partners918 AS P
		ON S.PartnerId = P.Id
WHERE CreateDate BETWEEN DATEADD(DAY, DATEDIFF(DAY,0, GETDATE())/7*7-7,0)
and DATEADD(DAY, DATEDIFF(DAY,0, GETDATE())/7*7-1,0) and O.[Status] like 'Исполнен' and I.Code is not null and I.Code not like ''
GROUP BY 
		C.[Name]
	   ,C.Id
	   ,Price
	   ,Amount
	   ,I.Code
	   ,I.[Name]
	   ,I.Article
	   ,I.Class
	   ,I.[Group]
	   ,I.Categoty
	   ,I.Manufacturer
	   ,I.SubCategory
	   ,I.TradeMark
	   ,S.Id
	   ,P.Id
	   ,P.[Name]
	   ,P.OrgAddress
ORDER BY S.Id ASC</property>
