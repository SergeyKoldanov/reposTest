                <property
                  dataType="System.String"
                  description="Команда SQL для выполнения."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT C.[Name] AS Клиент
	  ,C.Id AS [ID клиента]
	  ,S.Id AS [ID магазина партнера]
	  ,P.Name AS [Партнер]
	  ,P.Id AS [Код партнера]
