                  description="Команда SQL для выполнения."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
a77._Period  [Period] -- Период
   ,a77._Fld7744RRef GUIItem -- Гуид товара
   ,a77._Fld7746_RRRef GUIOrder -- Гуид заказа
   ,d149._Number NumberOrder -- Номер заказа
   ,a77._Fld7753 Quantity -- Количество
   ,a77._Fld7754 Total -- Сумма продажи
   ,d208._Fld4788RRef GUIBookmark -- Гуид закладки
   ,case when i641._Fld6412RRef is not null then 'True' else 'False' end as SignLoyalty 
   ,case when cast(d149._Fld15048 as bit) = 1 then 'True' else 'False' end as SignSite
   ,r70._Fld11590 NodeRIB --Узел РИБ
   ,r261._ParentIDRRef GUISiteChapterClassItem1 --Множественный классификатор для сайта - Гуид раздела классификатора 1 ур.
   ,t1._ParentIDRRef GUISiteChapterGroupItem2 -- Множественный классификатор для сайта - Гуид раздела классификатора 2 ур.
   ,t1._Fld26110RRef GUISiteChapterCategoryItem3 -- Множественный классификатор для сайта - Гуид раздела классификатора 3 ур.
   ,r53._Fld12557RRef GUIChapterClassItem1 -- Гуид раздела классификатора 1 ур.
   ,r53._Fld12558RRef GUIChapterClassItem2 -- Гуид раздела классификатора 2 ур.
   ,r125._ParentIDRRef GUIChapterClassItem3 -- Гуид раздела классификатора 3 ур.
   ,r53._Fld12559RRef GUIChapterClassItem4 -- Гуид раздела классификатора 4 ур.
   ,i217._Fld21723_RRRef GUIOldChapterClassItem1 -- Гуид раздела справочника номенклатуры, 1 ур
   ,r53._Fld845RRef GUIManufacter -- Производитель
   ,r53._Fld834RRef GUICountry -- Гуид страны
   ,r53._Fld12560RRef GUITrademark -- Товарная марка
   ,r53._Fld12562RRef GUISeries -- Гуид серии
   ,r53._Fld844RRef GUIMarkItem -- Гуид отметка товара
   ,r88._Code MarkCode -- Буквенный код отметки
   ,r53._Fld833RRef GUIVAT -- ГУИД Ставка НДС
   ,i64.HitSales -- Хит продаж
   ,i64.TOP150 -- ТОП 150
   ,i64.UnloadItem918 -- Выгружать товар на сайт 9-18
   ,r47._IDRRef GUIClient -- ГУИД Контрагента
   ,r47._Fld638RRef GUIHeadClient -- ГУИД головного контрагента
   ,r47._Fld11954 GUIClientNode -- Гуид основного узла клиента

FROM [sql-node-01].[RelefUTWork].[dbo]._AccumRg7743 a77 (nolock) 
LEFT JOIN [sql-node-01].[RelefUTWork].[dbo]._Document149 d149 (nolock) ON d149._IDRRef = a77._Fld7746_RRRef -- соединение со спраочником ... для получения номера заказа
LEFT JOIN [sql-node-01].[RelefUTWork].[dbo]._Document208 d208 (nolock) ON d208._IDRRef = a77._Fld7748_RRRef 
LEFT JOIN [sql-node-01].[RelefUTWork].[dbo]._InfoRg6410 i641 (nolock)  ON i641._Fld6411_RRRef = a77._Fld7746_RRRef and i641._Fld6412RRef = 0x8ECEB97EE6E44FC74DB0D079C9E4297C --признак лояльности
LEFT JOIN [sql-node-01].[RelefUTWork].[dbo]._Reference70 r70 (nolock) ON r70._IDRRef = a77._Fld10714RRef -- соединение со справочником слкада, откуда берём название склада и его узел
LEFT JOIN [sql-node-01].[RelefUTWork].[dbo]._Reference53 r53 (nolock) ON a77._Fld7744RRef = r53._IDRRef -- соединение со справочником номенклатуры
LEFT JOIN [sql-node-01].[RelefUTWork].[dbo]._Reference12528 r125 (nolock) ON r53._Fld12559RRef  = r125._IDRRef 
LEFT JOIN [sql-node-01].[RelefUTWork].[dbo]._Reference88 r88 (nolock) ON r53._Fld844RRef = r88._IDRRef -- соединение со справочником отметка товара
LEFT JOIN [sql-node-01].[RelefUTWork].[dbo]._Reference47 r47 (nolock) ON r47._IDRRef = a77._Fld7752RRef -- соединение со справочником контрагентов
LEFT JOIN (SELECT a._Fld22374_RRRef, a._Fld21723_RRRef FROM [sql-node-01].[RelefUTWork].[dbo]._InfoRg21722  a (nolock) 
	   WHERE a._Fld21723_RRRef in  (0x8AEA00E098C184C111DDC30A4FF76E7A, 0xB395E4115BD714A811E2146B7D954204, 0xB395E4115BD714A811E2146B7D954205)) i217 ON r53._ParentIDRRef = i217._Fld22374_RRRef
LEFT JOIN (SELECT ROW_NUMBER() OVER(PARTITION BY i26._Fld26111RRef ORDER BY r26._Fld28675 DESC) AS r, i26._Fld26111RRef, i26._Fld26110RRef, r26._ParentIDRRef, r26._Fld28675 
	   FROM [sql-node-01].[RelefUTWork].[dbo]._InfoRg26108 i26 (nolock)  
	   LEFT JOIN [sql-node-01].[RelefUTWork].[dbo]._Reference26106  r26 (nolock)  ON i26._Fld26110RRef = r26._IDRRef 
	   WHERE i26._Fld26109RRef = 0x80C230E1715C531711EAD2FD2FA5C39D) t1 ON t1._Fld26111RRef=r53._IDRRef and r = 1
LEFT JOIN [sql-node-01].[RelefUTWork].[dbo]._Reference26106  r261 (nolock) ON r261._IDRRef = t1._ParentIDRRef
LEFT JOIN (SELECT   a._Fld6411_RRRef
			,MAX(case when a._Fld6412RRef = 0x8DDF461E8BA7E65F424E62672B9FD282 then 'True' else 'False' end) as HitSales -- хит продаж
			,MAX(case when a._Fld6412RRef = 0x9405ACF1DF3FF64B11E7291E25E101E5 then 'True' else 'False' end) as TOP150 -- топ 150
			,MAX(case when a._Fld6412RRef = 0x8358F870BD65C12E450B54D7F81D1654 then 'True' else 'False' end) as UnloadItem918 -- 918 )
           FROM [sql-node-01].[RelefUTWork].[dbo]._InfoRg6410 a (nolock) 
		   GROUP BY a._Fld6411_RRRef) i64 ON i64._Fld6411_RRRef = a77._Fld7744RRef 
WHERE a77._Period &gt;= DATEADD(DAY,-90,GETDATE())</property>
