                  description="Указывает имя объекта базы данных, используемого для открытия набора строк."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Указывает переменную, содержащую имя объекта базы данных, используемого для открытия набора строк."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="Команда SQL для выполнения."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH Nom AS (
SELECT _IDRRef, _ParentIDRRef, _Code, _Description, _Description as Родитель, _Fld816, _Fld844RRef, _Fld845RRef, _Fld12557RRef, _Fld12558RRef, _Fld12559RRef, _Fld12560RRef, _Fld833RRef
FROM [RelefUTWork].[dbo].[_Reference53] WITH (NOLOCK)
WHERE _IDRRef in (0x8AEA00E098C184C111DDC30A4FF76E7A, 0xB395E4115BD714A811E2146B7D954204, 0xB395E4115BD714A811E2146B7D954205)

UNION ALL

SELECT N._IDRRef, N._ParentIDRRef, N._Code, N._Description, Nom.Родитель, N._Fld816, N._Fld844RRef, N._Fld845RRef, N._Fld12557RRef, N._Fld12558RRef, N._Fld12559RRef, N._Fld12560RRef, N._Fld833RRef
FROM Nom INNER JOIN [RelefUTWork].[dbo].[_Reference53] N (NOLOCK) 
	ON Nom._IDRRef = N._ParentIDRRef
)

  SELECT
	Nom._Code AS [Код],
	Nom._Description AS [Наименование],
	Nom._Fld816 AS [Артикул],
	O._Code AS [Отметка],
	P._Description AS [Производитель],
	cast(CASE 
	WHEN Nom.Родитель = 'Канцелярские товары' THEN 'Канцтовары'
	WHEN Nom.Родитель = 'Товары офисного назначения (ТОН)' THEN 'ТОН'
	WHEN Nom.Родитель = 'Бумага для оргтехники' THEN 'Бумага'
	END as nvarchar) AS Направление,
	K._Description AS [Класс],
	G._Description AS [Группа],
	T.Категория,
	T.Подкатегория,
	M._Description AS [Торговая марка],
	CASE
	WHEN ND._EnumOrder = 2 THEN 0.1
	WHEN ND._EnumOrder = 3 THEN 0.1
	WHEN ND._EnumOrder = 4 THEN 0
	WHEN ND._EnumOrder = 5 THEN 0
	WHEN ND._EnumOrder = 6 THEN 0.2
	ELSE 0
	END AS NDS,
	cast(CASE
	WHEN TT._IDRRef IS NOT NULL THEN 'Да'
	ELSE 'Нет'
	END as nvarchar) AS RSL
FROM Nom
	LEFT JOIN [RelefUTWork].[dbo].[_Reference88] O WITH (NOLOCK)
		ON Nom._Fld844RRef = O._IDRRef
	LEFT JOIN [RelefUTWork].[dbo].[_Reference91] P WITH (NOLOCK)
		ON Nom._Fld845RRef = P._IDRRef
	LEFT JOIN [RelefUTWork].[dbo].[_Reference12505] K WITH (NOLOCK)
		ON Nom._Fld12557RRef = K._IDRRef
	LEFT JOIN [RelefUTWork].[dbo].[_Reference12527] G WITH (NOLOCK)
		ON Nom._Fld12558RRef = G._IDRRef
	LEFT JOIN
			(SELECT 
					Cat._IDRRef AS [Ссылка],
					Cat._Description AS [Подкатегория],
					Cat1._Description AS [Категория]
			 FROM [RelefUTWork].[dbo].[_Reference12528] AS Cat WITH (NOLOCK)
				LEFT JOIN [RelefUTWork].[dbo].[_Reference12528] AS Cat1 WITH (NOLOCK)
					ON Cat1._IDRRef = Cat._ParentIDRRef) AS T
		ON Nom._Fld12559RRef = T.Ссылка
	LEFT JOIN [RelefUTWork].[dbo].[_Reference12504] M WITH (NOLOCK)
		ON Nom._Fld12560RRef= M._IDRRef
	LEFT JOIN [RelefUTWork].[dbo].[_Enum368] ND WITH (NOLOCK)
		ON Nom._Fld833RRef = ND._IDRRef
	LEFT JOIN 	(SELECT KK._Reference53_IDRRef, KKK._IDRRef
				 FROM [RelefUTWork].[dbo].[_Reference53_VT12585] KK WITH (NOLOCK) --ТАБЛ КЛАССИФ КАТАЛОГ	
					LEFT JOIN [RelefUTWork].[dbo].[_Reference12507] KKK WITH (NOLOCK)--СПР КЛАССИФ КАТАЛОГ
						ON KK._Fld12587RRef = KKK._IDRRef
				 WHERE KKK._Code like 'РЗН000002') TT
		ON Nom._IDRRef = TT._Reference53_IDRRef</property>
                <property
