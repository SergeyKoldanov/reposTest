              description="Потребляет данные из SQL Server, OLE DB, ODBC или Oracle с помощью соответствующего поставщика данных .NET. Используйте инструкцию Transact-SQL для определения результирующего набора. Пример: извлечение данных из SQL Server с помощью поставщика данных .NET Framework для SQL Server."
              name="9-18"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Задает инструкцию SQL, используемую компонентом для извлечения данных."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SET SESSION TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;
SELECT
ok_order_products.ID AS ID,
	ok_order.ID  as OrderID,
   UF_SHOP_CLIENT AS ClientID,
    ok_order.UF_CREATE_DATE AS DateOrder,
    ok_order.UF_SHOP_ID AS ShopID,
    cast(ok_order_products.UF_PRODUCT_XML_ID as char(50)) AS ItemGUID,
    ok_order_products.UF_QUANTITY AS Amount,
    ok_order_products.UF_PRICE AS Price,
    b_user_field_enum.VALUE AS Status,
case
when ok_order.UF_CREATE_DATE &lt; '20220401' then null
else ok_order_products.uf_buying_price 
end as Sebest
    
FROM ok_order_products LEFT JOIN ok_order
							ON ok_order_products.UF_ORDER = ok_order.ID
                       LEFT JOIN b_user_field_enum
                       		ON ok_order.UF_SHOP_STATUS = b_user_field_enum.ID
WHERE ok_order.UF_CREATE_DATE &gt;= DATE_SUB(CURDATE(), INTERVAL 32 DAY);
SET SESSION TRANSACTION ISOLATION LEVEL REPEATABLE READ;</property>
