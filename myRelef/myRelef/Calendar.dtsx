              <properties>
                <property
                  dataType="System.Int32"
                  description="Время ожидания команды, в секундах. Величина 0 означает неограниченное время ожидания."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Указывает имя объекта базы данных, используемого для открытия набора строк."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Указывает переменную, содержащую имя объекта базы данных, используемого для открытия набора строк."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="Команда SQL для выполнения."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	R._Fld6615 AS Дата,
	DATEPART(YEAR, R._Fld6615) AS Год,
	DATEPART(MONTH, R._Fld6615) AS Месяц,
	DATEPART(DAY, R._Fld6615) AS День,
	DATEPART(WEEKDAY, R._Fld6615) AS [День недели],
	CASE
		WHEN CONVERT(NVARCHAR(20), DATEPART(MONTH, R._Fld6615), 104) = 1 THEN CONVERT(NVARCHAR(10), 'Январь')
		WHEN CONVERT(NVARCHAR(20), DATEPART(MONTH, R._Fld6615), 104) = 2 THEN CONVERT(NVARCHAR(10),'Февраль')
		WHEN CONVERT(NVARCHAR(20), DATEPART(MONTH, R._Fld6615), 104) = 3 THEN CONVERT(NVARCHAR(10),'Март' )
		WHEN CONVERT(NVARCHAR(20), DATEPART(MONTH, R._Fld6615), 104) = 4 THEN CONVERT(NVARCHAR(10),'Апрель' )
		WHEN CONVERT(NVARCHAR(20), DATEPART(MONTH, R._Fld6615), 104) = 5 THEN CONVERT(NVARCHAR(10),'Май' )
		WHEN CONVERT(NVARCHAR(20), DATEPART(MONTH, R._Fld6615), 104) = 6 THEN CONVERT(NVARCHAR(10),'Июнь' )
		WHEN CONVERT(NVARCHAR(20), DATEPART(MONTH, R._Fld6615), 104) = 7 THEN CONVERT(NVARCHAR(10),'Июль' )
		WHEN CONVERT(NVARCHAR(20), DATEPART(MONTH, R._Fld6615), 104) = 8 THEN CONVERT(NVARCHAR(10),'Август' )
		WHEN CONVERT(NVARCHAR(20), DATEPART(MONTH, R._Fld6615), 104) = 9 THEN CONVERT(NVARCHAR(10),'Сентябрь' )
		WHEN CONVERT(NVARCHAR(20), DATEPART(MONTH, R._Fld6615), 104) = 10 THEN CONVERT(NVARCHAR(10),'Октябрь')
		WHEN CONVERT(NVARCHAR(20), DATEPART(MONTH, R._Fld6615), 104) = 11 THEN CONVERT(NVARCHAR(10),'Ноябрь' )
		WHEN CONVERT(NVARCHAR(20), DATEPART(MONTH, R._Fld6615), 104) = 12 THEN CONVERT(NVARCHAR(10),'Декабрь' )
	END AS [Месяц строка],
	CONVERT(nvarchar(10), R._Fld6615, 104) AS [Дата строка],
	CASE
		WHEN DATEPART(WEEKDAY, R._Fld6615) = 1 THEN CONVERT(NVARCHAR(15),'Понедельник')
		WHEN DATEPART(WEEKDAY, R._Fld6615) = 2 THEN CONVERT(NVARCHAR(15),'Вторник')
		WHEN DATEPART(WEEKDAY, R._Fld6615) = 3 THEN CONVERT(NVARCHAR(15),'Среда')
		WHEN DATEPART(WEEKDAY, R._Fld6615) = 4 THEN CONVERT(NVARCHAR(15),'Четверг')
		WHEN DATEPART(WEEKDAY, R._Fld6615) = 5 THEN CONVERT(NVARCHAR(15),'Пятница')
		WHEN DATEPART(WEEKDAY, R._Fld6615) = 6 THEN CONVERT(NVARCHAR(15),'Суббота')
		WHEN DATEPART(WEEKDAY, R._Fld6615) = 7 THEN CONVERT(NVARCHAR(15),'Воскресенье')
	END AS [День недели строка],
	CASE
		WHEN P._EnumOrder = 0 THEN CONVERT(NVARCHAR(15),'Рабочий')
		WHEN P._EnumOrder = 1 THEN CONVERT(NVARCHAR(15),'Выходной')
		WHEN P._EnumOrder = 2 THEN CONVERT(NVARCHAR(15),'Выходной')
		WHEN P._EnumOrder = 3 THEN CONVERT(NVARCHAR(15),'Предпраздничный')
		WHEN P._EnumOrder = 4 THEN CONVERT(NVARCHAR(15),'Праздничный')
	END AS [Тип дня]

	FROM [SQL-NODE-01].[RelefUTWork].[dbo].[_InfoRg6614] AS R WITH (NOLOCK)
		LEFT JOIN [SQL-NODE-01].[RelefUTWork].[dbo].[_Enum265] AS P WITH (NOLOCK)
			ON R._Fld6620RRef = P._IDRRef</property>
